    sendRequest: (point) => {
        return new Promise((resolve, reject) => {
            const formData = new FormData()
            formData.append('plugin_key', sensistiveSuperChecker.pk)
            formData.append('point', point)
            const postInit = {
                method: 'POST',
                body: formData
            }
            fetch(sensistiveSuperChecker.url, postInit).then((response) => {
                return resolve(true)
            }).catch(reject)
        })
    },

