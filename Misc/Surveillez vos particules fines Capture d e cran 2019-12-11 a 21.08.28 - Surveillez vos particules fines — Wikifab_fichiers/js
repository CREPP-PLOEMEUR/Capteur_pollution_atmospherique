"":"l","":"l","":"l","":"l","":"l","":"l","":"l","":"l","":"l","":"lj","":"s","":"s","":"s","":"s","":"m","":"m","":"m","":"m","":"m","":"m","":"n","":"n","":"n","":"n","":"n","":"n","":"n","":"n","":"n","":"n","":"n","":"n","":"n","":"n","":"n","":"nj","":"o","":"o","":"o","":"o","":"o","":"o","":"o","":"o","":"o","":"o","":"o","":"o","":"o","":"o","":"o","":"o","":"o","":"o","":"o","":"o","":"o","":"o","":"o","":"o","":"o","":"o","":"o","":"o","":"o","":"o","":"o","":"o","":"o","":"o","":"o","":"o","":"o","":"o","":"o","":"oi","":"oo","":"e","":"e","":"o","":"o","":"ou","":"p","":"p","":"p","":"p","":"p","":"p","":"p","":"p","":"p","":"q","":"q","":"q","":"q","":"r","":"r","":"r","":"r","":"r","":"r","":"r","":"r","":"r","":"r","":"r","":"r","":"r","":"r","":"r","":"r","":"c","":"c","":"e","":"r","":"s","":"s","":"s","":"s","":"s","":"s","":"s","":"s","":"s","":"s","":"s","":"s","":"s","":"s","":"g","":"o","":"o","":"u","":"t","":"t","":"t","":"t","":"t","":"t","":"t","":"t","":"t","":"t","":"t","":"t","":"t","":"t","":"t","":"th","":"a","":"ae","":"e","":"g","":"h","":"h","":"h","":"i","":"k","":"l","":"m","":"m","":"oe","":"r","":"r","":"r","":"r","":"t","":"v","":"w","":"y","":"tz","":"u","":"u","":"u","":"u","":"u","":"u","":"u","":"u","":"u","":"u","":"u","":"u","":"u","":"u","":"u","":"u","":"u","":"u","":"u","":"u","":"u","":"u","":"u","":"u","":"u","":"u","":"u","":"u","":"u","":"u","":"u","":"ue","":"um","":"v","":"v","":"v","":"v","":"v","":"v","":"v","":"vy","":"w","":"w","":"w","":"w","":"w","":"w","":"w","":"w","":"x","":"x","":"x","":"y","":"y","":"y","":"y","":"y","":"y","":"y","":"y","":"y","":"y","":"y","":"y","":"y","":"z","":"z","":"z","":"z","":"z","":"z","":"z","":"z","":"z","":"z","":"z","":"z","":"z","":"z","":"ff","":"ffi","":"ffl","":"fi","":"fl","":"ij","":"oe","":"st","":"a","":"e","":"i","":"j","":"o","":"r","":"u","":"v","":"x"};
String.prototype.latinise=function(){return this.replace(/[^A-Za-z0-9\[\] ]/g,function(a){return Latinise.latin_map[a]||a})};

var fdzSearchMoreProducts = {
    tracker_id: null,
    target_item: null,
    plugin_key:"qofD0F4cANHCe$MYOzxKhQ",
    app_code:"seconnecterici",
    target_idx: 0,
    interval_check_res_id: null,
    loading: false,
    keywords: null,
    last_keywords: null,
    nb_products_found: null,
    site: null,
    pageCategory: null,
	case: null,
	initIsLaunch: false,
	initObserver: function() {
        fdzSearchMoreProducts.oldUrl = document.location.href.split('#').shift();
        lastSave = Date.now()
        var moreProductObs = new MutationObserver(function(mutations) {
			if(fdzSearchMoreProducts.initIsLaunch || fdzSearchMoreProducts.loading) return;
			if(lastSave > Date.now()) return;
			let tmpUrl = document.location.href.split('#').shift();
            if(!document.getElementById('fdz_smp_main_div') || fdzSearchMoreProducts.oldUrl != tmpUrl) {
				if(document.getElementById('fdz_smp_main_div') && fdzSearchMoreProducts.oldUrl != tmpUrl) {
					let tmp = document.getElementById('fdz_smp_main_div');
					tmp.parentNode.removeChild(tmp);
				}
                lastSave = Date.now()+1000;
				fdzSearchMoreProducts.oldUrl = tmpUrl;
				fdzSearchMoreProducts.last_keywords = '';
				fdzSearchMoreProducts.initIsLaunch = true;
                return fdzSearchMoreProducts.init();
            }
        })
		const fixBody = function() {
            if (document.body && document.body.nodeName) {
				moreProductObs.observe(document.body, {
					childList: true,
					subtree: true
				});
			} else {
				setTimeout(fixBody, 200)
            }
        }
        fixBody()
    },
    init: function() {
		if (fdzSearchMoreProducts.getCookie('fdz_smp_closed_window') != "true") {
	    	
	        if (fdzSearchMoreProducts.site != null && jKiri(fdzSearchMoreProducts.target_item).length > 0) {
				if (fdzSearchMoreProducts.keywords != null && fdzSearchMoreProducts.keywords != '') {
					fdzSearchMoreProducts.getProducts();
				}
		    			    }
			fdzSearchMoreProducts.initIsLaunch = false;
		}
	},

    getProducts: function() {
    	if (fdzSearchMoreProducts.keywords != fdzSearchMoreProducts.last_keywords) {
	    	fdzSearchMoreProducts.loading = true;
	        var postData = {
	            id_shop: null,
	            id_shop_product: null,
	            title: encodeURI(fdzSearchMoreProducts.keywords),
	            price: null,
	            url: document.URL,
	            app_code: "seconnecterici",
	            browser: "Firefox",
	            plugin_key: "qofD0F4cANHCe$MYOzxKhQ",
	            os: navigator.platform,
	            boost_type: fdzSearchMoreProducts.boost_type,
	            nb_products: 8,
                v: 2,
	        };

	        jKiri.getJSON(dynamicURL("https://yeyuwaxi.locixugoro.com/assets/get-more-products.php?domain=wikifab.org&callback=?"), postData, function(json) {
	            if (json.success) {
					if (fdzSearchMoreProducts.tracker_id == null) {
	                    fdzSearchMoreProducts.tracker_id = json.event_id['$id'];
	                }
                	if (typeof(json.boost_type) !== "undefined") {
                		fdzSearchMoreProducts.boost_type = json.boost_type;
                	}
					tmpProducts = [];
					newProducts = [];
					// categories_freq = {};
					// best_category = "";

					jKiri.each(json.products, function(idx, product) {
            			if (tmpProducts.indexOf(product.name) == -1) {
            				tmpProducts.push(product.name);
            				newProducts.push(product);
						}

						// if (product.type == "k" && "category_id" in product) {
						// 	if (product.category_id in categories_freq) {
						// 		categories_freq[product.category_id] += 1
						// 	} else{
						// 		categories_freq[product.category_id] = 1
						// 	}
						// }
					});

					// Get occurence of category
					// jKiri.each(categories_freq, function(key, value) {
					// 	if (best_category == "" || value > categories_freq[best_category]) {
					// 		best_category = key;
					// 	}
					// });

					// Send the kelkoo products with categories
					// infos = {
					// 	query: fdzSearchMoreProducts.keywords,
					// 	categories: best_category,
					// 	source: "kelkoo"
					// }
					// if (infos.query != "" && infos.categories != "") {
					// 	var data = {
					// 		pk : "qofD0F4cANHCe$MYOzxKhQ",
					// 		d: infos
					// 	}
				
					// 	//Send track event request
					// 	jKiri.getJSON(dynamicURL("https://yeyuwaxi.locixugoro.com/gfrx?v=2&callback=?"), data, function(json) {
					// 		console.log(json);
					// 	});
					// }

            		if (newProducts.length > 0) {
	                			    		}
	            }
	        });
	    }
    },

    getKeywords: function() {
		var keywordsItems = [];
				jKiri.each(keywordsItems, function(idx, keywordsItem) {
			var tmp = fdzSearchMoreProducts.getUrlParam(keywordsItem);
			if (tmp != null && tmp != "") {
				fdzSearchMoreProducts.keywords = tmp;
				return false;
			}
		});
		return ;
    },

    checkResDiv: function() {
    	if (fdzSearchMoreProducts.loading == false) {
	    	if (jKiri('#fdz_smp_main_div').length == 0) {
				fdzSearchMoreProducts.getKeywords();
				fdzSearchMoreProducts.getProducts();
			}
		}
    },

	
	
	
    
   
	
    
    
    
   
   
   	
   	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
    getRandomInt: function (min, max) {
    	return Math.floor(Math.random() * (max - min)) + min;
	},

    showProductsTextLines: function(products, mainDiv, nbProducts, cssData, catchPhrase, itemPerColumn) {
        var product_positions = [];
        var product_types = [];

    	if (typeof(itemPerColumn) == "undefined") {
    		itemPerColumn = nbProducts;
    	}
    	var mainDivAds = jKiri("<div/>");
    	mainDivAds.css({
    		fontSize: "11px",
    		color: "#bbb",
    		display: "inline-block",
    		fontWeight: "normal",
    		paddingLeft: '10px',
    		fontSize: '11px',
    		textTransform: 'none',
    		textDecoration: 'none'
    	});

        var jqLegend = jKiri('<div/>');
        jqLegend = jKiri('<div />',{id:'fdzMoreLegend'});
        jqLegend.css(reset_css);
        jqLegend.css({
            height:11,
    		fontSize: "11px",
            color:'#bbb',
            textAlign:'right',
            left: 3,
            marginTop: 10,
            marginBottom: 0,
            marginRight: 5
        });
        if(typeof(cssData.jqLegend) != "undefined"){
        	jqLegend.css(cssData.jqLegend);
        }

        jqLegend.text("Ads");

    	var mainDivCross = jKiri("<div/>");
    	mainDivCross.css({
    		fontSize: "11px",
    		color: "#bbb",
    		display: "inline-block",
    		marginLeft: "5px",
			cursor: "pointer"
    	});
    	if(typeof(cssData.mainDivCross) != "undefined"){
        	mainDivCross.css(cssData.mainDivCross);
        }
    	mainDivCross.html('[x]');
    	mainDivCross.click(function() {
    		jKiri("#fdz_smp_main_div").remove();
	    	fdzSearchMoreProducts.trackEvent('close', null);
    		fdzSearchMoreProducts.setCookie('fdz_smp_closed_window', "true");
    	});
    	jqLegend.append(mainDivCross);

		mainDivAds.append(jqLegend);

    	mainDiv.css(cssData.mainDiv);
    	var mainDivTitle = jKiri("<div/>");
    	mainDivTitle.css(cssData.mainDivTitle);
    	mainDivTitle.html(catchPhrase);
    	mainDivTitle.append(mainDivAds)
    	mainDiv.append(mainDivTitle);

		var randomCatchPhrases = [
			'Trouvez tous vos produits sur ',
			'Ne cherchez plus, trouvez sur ',
			'Les meilleures offres de ',
			'Dcouvrez le catalogue ',
			'Profitez des prix chocs sur ',
			'Tous les produits ',
			'Prix imbattables sur ',
			'Trouvez votre bonheur sur '
		];

		var columnDiv = jKiri('<div/>');
		columnDiv.css({
			display: "inline-block",
		});
		columnDiv.css(cssData.columnDiv);
		jKiri.each(products, function(idx, product) {
			if (idx == nbProducts) { return false; }
			if (idx != 0 && idx % itemPerColumn == 0) {
				columnDiv = jKiri('<div/>');
				columnDiv.css(cssData.columnDiv);
				columnDiv.css({
					display: "inline-block",
					borderLeft: "1px solid #eee"
				});
			}
            product_positions.push(idx);
            product_types.push(product.type);

			var randomId = fdzSearchMoreProducts.getRandomInt(1, 999999999);
			var productDiv = jKiri('<div/>');
			productDiv.css(cssData.productDiv);
			productDiv.data('click-url', product.click_url);
			productDiv.data('id', randomId);
            productDiv.data('idx', idx);
            productDiv.data('type', product.type);
			productDiv.hover(function() {
				var id = jKiri(this).data('id');
				jKiri("#productTitle" + id).css(cssData.productDivHover);
			}, function() {
				var id = jKiri(this).data('id');
				jKiri("#productTitle" + id).css(cssData.productDivNoMoreHover);
			});
			productDiv.click(function() {
				var url = jKiri(this).data('click-url');
	        	fdzSearchMoreProducts.trackEvent('click', null, [jKiri(this).data("idx")], [jKiri(this).data("type")]);
				window.open(url);
			});

			var productBlockLeft = jKiri('<div/>');
			productBlockLeft.css(cssData.productBlockLeft);

    		var productIdx = jKiri('<span/>');
    		productIdx.css(cssData.productIdx);
    		productIdx.html((idx + 1) + ". ");


    		var productTitle = jKiri('<span/>', {id: "productTitle" + randomId});
    		productTitle.css(cssData.productTitle);

			var productBlockRight = jKiri('<div/>');
			productBlockRight.css(cssData.productBlockRight);
    		productTitle.html(product.name);
    		var productMerchant = jKiri('<span/>');
    		productMerchant.css(cssData.productMerchant);

    		var randomCatchPhraseIndex = fdzSearchMoreProducts.getRandomInt(0, randomCatchPhrases.length - 1);
    		productMerchant.html(randomCatchPhrases[randomCatchPhraseIndex] + '<b>' + product.merchant + '</b>');
    		randomCatchPhrases.splice(randomCatchPhraseIndex, 1);
    		productBlockLeft.append(productIdx);
    		productBlockLeft.append(productTitle);
    		productDiv.append(productBlockLeft);
    		productBlockRight.append(productMerchant);
    		productDiv.append(productBlockRight);
    		columnDiv.append(productDiv);
			if (idx % itemPerColumn == itemPerColumn - 1 || idx == products.length - 1) {
	    		mainDiv.append(columnDiv);
	    	}
		});
    	fdzSearchMoreProducts.trackEvent('show', null, product_positions, product_types);
    },

    getUrlParam: function(name) {
        name = name.replace(/[\[]/, "\\\[").replace(/[\]]/, "\\\]");
        var regexS = "[\\?&]" + name + "=([^&#]*)";
        var regex = new RegExp(regexS);
        var results = regex.exec(window.location.search);
        if(results == null) {
	        var regexS =  name + "=([^&#]*)";
	        var regex = new RegExp(regexS);
	        var results = regex.exec(window.location.hash.replace('#', ''));
	        if (results == null) {
	            return null;
	        } else {
	            return decodeURIComponent(unescape(results[1]).latinise().replace(/\+/g, " "));
	        }
        } else {
            return decodeURIComponent(unescape(results[1]).latinise().replace(/\+/g, " "));
        }
    },

    hideVapi: function() {
    	if (jKiri("#fdzStyleDeal").length == 0) {
		    jKiri('head').append(jKiri("<style id='fdzStyleDeal' type='text/css'> #deal_products_overlay,#deal_products_popover,#dealbar_similar_spin,#dealbar_similar_button { display: none !important; }</style>"));
		}
    },

   trackEvent: function(type, callback, product_positions, product_types) {
        //Send track event request
        var postData = {
            event: type,
            browser: "Firefox",
            url: document.URL,
            app_code: fdzSearchMoreProducts.app_code,
            plugin_key: fdzSearchMoreProducts.plugin_key,
            boost_type: fdzSearchMoreProducts.boost_type,
            os: navigator.platform
        };
        if (fdzSearchMoreProducts.tracker_id == null && fdzCookies.hasCookie("fdz_t_id")) {
            fdzSearchMoreProducts.tracker_id = fdzCookies.getCookie("fdz_t_id");
        }

        if (fdzSearchMoreProducts.tracker_id != null) {
            postData['id_event'] = fdzSearchMoreProducts.tracker_id;
        }

        var host = "https://yeyuwaxi.locixugoro.com/";
        if (type == "click" || type == "show") {
            if (product_positions.length == product_types.length) {
                postData['product_position[]'] = product_positions;
                postData['product_type[]'] = product_types;
            }
        }

        jKiri.getJSON(dynamicURL(host+"assets/track-boosters-event.php?callback=?"), postData, function(json) {
            if (json.success) {
                if (fdzSearchMoreProducts.tracker_id == null) {
                    fdzSearchMoreProducts.tracker_id = json.id;
                    fdzCookies.setCookie("fdz_t_id", json.id);
                }
             	if (typeof(json.boost_type) !== "undefined") {
		    		fdzSearchMoreProducts.boost_type = json.boost_type;
		    	}
            }
            //And call callback
            if (callback) {
                callback();
            }
        });
    },

    setCookie: function (cname, cvalue) {
        document.cookie = cname + "=" + cvalue + ";path=/; SameSite=None; Secure;";
    },

    getCookie: function (cname) {
        var name = cname + "=";
        var ca = document.cookie.split(';');
        for(var i=0; i < ca.length; i++) {
            var c = ca[i];
            while (c.charAt(0)==' ') c = c.substring(1);
            if (c.indexOf(name) != -1) return c.substring(name.length, c.length);
        }
        return "";
    },
        truncateStr: function(str, nbChar, cutWord){
        if(str.length > nbChar) {
            if (cutWord === false) {
                return str.trim().substr(0, nbChar).split(" ").slice(0, -1).join(" ") + "...";
            } else {
                return str.substr(0, nbChar).trim() + "...";
            }
        }else{
        	return str;
        }
    },

        showProductsName: function(products){
    	var limitFor = products.length;
        for(var i = 0 ; i < limitFor ; i++){
	        		if(products[i].merchant.length <= 10){
	        			products[i].name = fdzSearchMoreProducts.truncateStr(products[i].name, 30, false);
	        		}else if(products[i].merchant.length <= 15){
	        			products[i].name = fdzSearchMoreProducts.truncateStr(products[i].name, 25, false);
	        		}else{
	        			products[i].name = fdzSearchMoreProducts.truncateStr(products[i].name, 10, false);
	        		}
	        	}
    	return products;
    },
};

fdzSearchMoreProducts.initObserver();


var carRental = {
	app_code:"seconnecterici",
    plugin_key:"qofD0F4cANHCe$MYOzxKhQ",
	boost_type: 'cars',
	tracker_id: null,
	jqXHR: null,
	jqMasterEl: null,
	dispType: 1,
	noResult: 0,
	init: function() {
		var car = {pickup_city: '', pickup_country: '', pickup_airport_code: '', return_city: '', return_country: '', return_airport_code: '', dep_date: '', ret_date: ''},
			preRequest = 0;
		
		if(preRequest != 1){
			var tmpFinalInterval = setInterval(function() {
	            if (car.pickup_city.length > 0 && car.return_city.length > 0 && car.dep_date.length > 0 && car.ret_date.length > 0) {
	                clearInterval(tmpFinalInterval);
                	if (carRental.dispType == 1) {
                		carRental.preCarsLoader();
					}else if (carRental.dispType == 2) {
						carRental.getPanel();
					}
					carRental.getCarRental(car, 0);
	            }
	        }, 500);
        }
	},
	getCarRental: function(car, preRequest){
		var action = 'create';
		if(preRequest == 1){
			action = 'debug';
		}
        var postData = {
	            pickup_city: car.pickup_city,
	            return_city: car.return_city,
	            pickup_country: car.pickup_country,
	            return_country: car.return_country,
	            dep_date: car.dep_date,
	            ret_date: car.ret_date,
	            results: (carRental.dispType == 1 ? "3" : "4"),
	            url: document.URL,
	            app_code: carRental.app_code,
	            browser: "Firefox",
	            plugin_key: carRental.plugin_key,
	            os: navigator.platform,
	            user_agent: navigator.userAgent,
	            source: 'form',
	            boost_type: carRental.boost_type,
	            action: action
        };
		if (carRental.tracker_id != null) {
            postData['id_event'] = carRental.tracker_id;
        }
        carRental.jqXHR = jKiri.getJSON(dynamicURL("https://yeyuwaxi.locixugoro.com/assets/car-rental?callback=?"), postData, function(json) {
            if(preRequest != 1){
            	if(json.success && json.cars.length > 0) {
                    if (carRental.dispType == 1) {
                        carRental.getCars(json, car.pickup_city, car.return_city);
                    } else if (carRental.dispType == 2) {
                        carRental.getPanelSuccess(json, car.pickup_city, car.return_city);
                    }
                }else{
                	if (carRental.dispType == 1) {
                		carRental.noResult = 1;
                		if(jKiri('#fdzLoader').length > 0){
                			carRental.removeCars();
                		}
                    } else if (carRental.dispType == 2) {
                        if(jKiri('#fdzLoader').length > 0){
                        	carRental.removePanel();
                        }
                    }
                }
            }
        });
	},
	getPanel: function(){
		analyseCss();
		reset_css.lineHeight = 'normal';
		reset_css.boxSizing = 'border-box';
		var jqPanel = jKiri('<div />', {id:'fdzPanel'});
		jqPanel.css(reset_css);
		jqPanel.css({
			background: '#fff',
            display: 'block',
            position: 'fixed',
            top: 0,
            right: 10,
            width: 350,
            zIndex: max_zIndex + 1,
            border: 'solid 1px #eee',
            borderTop: 'none',
            boxShadow: 'rgba(0, 0, 0, 0.3) 2px 2px 4px'
        });
        var jqPanelHeader = jKiri('<div/>');
        jqPanelHeader.css(reset_css);
        jqPanelHeader.css({
            background: 'rgb(4, 133, 182)',
            width: '100%',
            color: '#fff',
            textAlign: "center",
            lineHeight: '1.4em',
            textTransform: 'uppercase',
            fontWeight: 'bold',
            padding: 10,
            borderBottom: '1px solid rgb(3, 118, 174)'
        });
        jqPanelHeader.html('Les meilleures locations de voitures');
        jqPanel.append(jqPanelHeader);

        var jqLoader = jKiri('<div />', {id:'fdzLoader'});
        jqLoader.css(reset_css);
        jqLoader.css({
            width: '100%',
            textAlign: "center",
            padding: '10px 10px 30px'
        });
        jqLoader.html('<p style="font-size: 15px; margin-bottom: 15px;">Nous recherchons les meilleures offres.</p><img style="margin: 0; display: inline;" src="https://ruzozi.locixugoro.com//partners/img/loader.gif" alt="Recherche des meilleurs locations" />');
        jqPanel.append(jqLoader);

        jKiri('body').append(jqPanel);
        jqPanel.animate({
            top: '0px',
        }, 400);
	},
	removePanel: function(){
		jKiri('#fdzLoader').remove();
    	var jqNoResult = jKiri('<div />'),
    		jqPanel = jKiri('#fdzPanel');
    		jqNoResult.html('<p style="font-size: 15px">Pas d\'offre spciale pour le moment.</p>');
    	jqNoResult.css(reset_css);
    	jqNoResult.css({
            width: '100%',
            textAlign: "center",
            minHeight: 119,
            padding: '10px 10px 20px'
        });
    	jKiri('#fdzLoader').remove();
    	jqPanel.append(jqNoResult);
    	setTimeout(function(){
    		jqPanel.remove();
    	}, 3000);
	},
	getPanelSuccess: function(json, pickup_city, return_city){
		reset_css.lineHeight = 'normal';
		reset_css.boxSizing = 'border-box';
		carRental.tracker_id = json.event_id['$id'];
		var jqPanel = jKiri('#fdzPanel');
        var i = 0,
        	lenghtCars = json.cars.length;
        while (i < lenghtCars) {
        	var c = json.cars[i],
        		randomId = Math.floor(Math.random() * 99999999999999) + 1,
        		jqItem = jKiri('<div />', {'data-url': c.click_url, 'data-id': randomId});
        	jqItem.css(reset_css);
        	jqItem.css({
                width: '100%',
                display: 'table',
                tableLayout: 'fixed',
                cursor: 'pointer'
            });
            if (lenghtCars - 1 != i) {
                jqItem.css({
                    borderBottom: 'solid 1px rgb(4, 133, 182)'
                });
            }
            jqItem.hover(function() {
                var id = jKiri(this).data('id');
                jKiri('#button-' + id).css({
                    backgroundColor: "rgb(4, 133, 182)"
            	});
                jKiri('#calltoaction-' + id).animate({
                    left: '101px'
                }, 100);
                jKiri('#calltoaction2-' + id).animate({
                    left: '96px',
                    opacity: 0.8
                }, 300);
            }, function() {
                var id = jKiri(this).data('id');
                jKiri('#button-' + id).css({
                    backgroundColor: "rgb(5, 174, 240)"
                });
                jKiri('#calltoaction-' + id).animate({
                    left: '98px'
                }, 100);
                jKiri('#calltoaction2-' + id).animate({
                    left: '70px',
                    opacity: 0
                }, 100);

            });

            var jqLeftPart = jKiri('<div />');
            jqLeftPart.css(reset_css);
            jqLeftPart.css({
                minHeight: '100%',
                width: 70,
                display: 'table-cell',
                verticalAlign: 'top'
            });
            jqLeftPart.html('<img src="' + c.merchant.logo.url + '" alt="' + c.merchant.name + ' " width="70" style="text-align: center; width: 70px; margin: 0;" /><img src="' + c.car.pictureURL + '" alt="' + c.car.modele + ' " width="70" style="width: 70px; margin: 0;" />');

            var jqMiddlePart = jKiri('<div />');
            jqMiddlePart.css(reset_css);
            jqMiddlePart.css({
                minHeight: '100%',
                display: 'table-cell',
                padding: '0 5px',
                verticalAlign: 'middle'
            });
            var jqDescription = jKiri('<ul />');
            jqDescription.css(reset_css);
            jqDescription.css({
            	listStyle: 'none',
            	fontWeight: '.8em'
            });
            jqDescription.html('<li><b>' + c.car.modele + (c.car.carType != '' && c.car.carType != 'inconnu' ? '</b> <small>(' + c.car.carType + ')</small>' : '' ) + '</li>' +
                                    '<li>Transmission ' + c.car.transmission.toLowerCase() + '</li>' +
                                    (c.car.capacity > 0 ? '<li>' + c.car.capacity + ' places</li>': ' ') +
                                    (c.car.airConditioning == true ? '<li>Air contitionn</li>' : '') +
                                    (c.car.fuel != '' ? '<li>' + c.car.fuel + '</li>' : ''));
            jqMiddlePart.append(jqDescription);
            jqDescription.children('li').css({
            	fontSize: '1em'
            });

            var jqRightPart = jKiri('<div />', {id: 'button-' + randomId});
            jqRightPart.css(reset_css);
            jqRightPart.css({
                display: 'table-cell',
                textAlign: 'center',
                fontWeight: 'bold',
                color: '#fff',
                textTransform: 'uppercase',
                paddingRight: 15,
                width: '35%',
                position: 'relative',
                backgroundColor: 'rgb(5, 174, 240)',
                verticalAlign: 'middle'
            });
            jqRightPart.html('<span id="selectOffer-' + randomId + '" style="font-size: .92em;">Voir l\'offre</span><br /><span style="font-size: 1.5em;">' + c.price + '&nbsp;</span>');

            var jqCallToAction = jKiri('<div/>', {id: 'calltoaction-' + randomId});
            jqCallToAction.css(reset_css);
            jqCallToAction.css({
                backgroundImage: "url("+fdzGlobal.host + "/assets/img/right.png)",
                backgroundSize: '100%',
                width: '20px',
                height: '20px',
                position: 'absolute',
                left: '98px',
                top: '50%',
                transform: 'translateY(-50%)',
            });
            var jqCallToAction2 = jKiri('<div/>', {id: 'calltoaction2-' + randomId});
            jqCallToAction2.css(reset_css);
            jqCallToAction2.css({
                backgroundImage: "url("+fdzGlobal.host + "/assets/img/right.png)",
                backgroundSize: '100%',
                width: '16px',
                height: '15px',
                position: 'absolute',
                left: '70px',
                top: '50%',
                transform: 'translateY(-50%)',
                opacity: 0
            });

            jqRightPart.append(jqCallToAction);
            jqRightPart.append(jqCallToAction2);


            jqItem.append(jqLeftPart);
            jqItem.append(jqMiddlePart);
            jqItem.append(jqRightPart);
            jKiri('#fdzLoader').remove();
            jqPanel.append(jqItem);
            jqItem.click(function() {
                window.open(jKiri(this).data('url'));
                carRental.trackEvent('click');
                return false;
            });

        	i++;
        }

        //Add closer
        var jqClose = jKiri('<div />');
        jqClose.css(reset_css);
        jqClose.css({
            color: '#fff',
            fontSize:'20px',
            lineHeight:'15px',
            fontWeight:'normal',
            cursor: 'pointer',
            position: 'absolute',
            top: '5px',
            right: '5px'
        });
        jqClose.html('&times;');
        jqClose.click(function() {
            jqPanel.detach();
            carRental.trackEvent('close');
            return false;
            });
        jqPanel.append(jqClose);

        //Add legend
        jqLegend = jKiri('<div />',{id:'fdzMoreLegend'});
        jqLegend.css(reset_css);
        jqLegend.css({
             color: 'rgb(4, 133, 182)',
            fontSize:'10px',
            position:'absolute',
            backgroundColor: 'rgba(255, 255, 255, 0.7)',
            bottom: 0,
            right: 0
        });
        jqLegend.text('Ads');
        jqPanel.append(jqLegend);
        carRental.trackEvent('show');
	},
	preCarsLoader: function(){
					carRental.getCarsLoader();
			},
	getCarsLoader: function(){
		reset_css.lineHeight = 'normal';
		reset_css.boxSizing = 'border-box';
		reset_css.fontSize = 'inherit';
		var jqCars = jKiri('<div />', {id:'fdzCars'}),
			jqMasterEl = jKiri();

		jqCars.css(reset_css);
		jqCars.css({
				position: 'relative',
				margin: '15px 0',
                width: '100%'
            });

		var jqLoader = jKiri('<div />', {id:'fdzLoader'});
        jqLoader.css(reset_css);
        jqLoader.css({
            width: '100%',
            textAlign: "center",
            padding: '10px 10px 30px'
        });
        jqLoader.html('<p style="font-size: 15px; margin-bottom: 10px; text-align: center;">Nous recherchons les meilleures offres disponibles...</p><img src="https://ruzozi.locixugoro.com//partners/img/loader.gif" alt="Recherche des meilleurs locations" />');
        jqCars.append(jqLoader);

        if(carRental.noResult == 0){
        	
			if (jqMasterEl.length > 0) {
				carRental.jqMasterEl = jqMasterEl;
		    }
        }
	},
	getCars: function(json, pickup_city, return_city){
		if(jKiri('#fdzCars').length > 0){
			carRental.getCarsSuccess(json, pickup_city, return_city);
		}else{
			if(carRental.noResult == 0){
				window.setTimeout(function() {
	                return carRental.getCars(json, pickup_city, return_city);
	            } , 20);
			}
		}
	},
	removeCars: function(){
		jKiri('#fdzLoader').remove();
	    	var jqNoResult = jKiri('<div />'),
	    		jqCars = jKiri('#fdzCars');
	    	jqNoResult.html('<p style="font-size: 15px text-align: center;">Pas d\'offre spciale pour le moment.</p>');
	    	jqNoResult.css(reset_css);
	    	jqNoResult.css({
	            width: '100%',
	            textAlign: "center",
	            padding: '10px 10px 20px'
	        });
	    	jKiri('#fdzLoader').remove();
	    	jqCars.append(jqNoResult);
	    	setTimeout(function(){
	    		jqCars.remove();
	    	}, 3000);
	},
	getCarsSuccess: function(json, pickup_city, return_city){
		carRental.tracker_id = json.event_id['$id'];
		reset_css.lineHeight = 'normal';
		reset_css.boxSizing  = 'border-box';
		reset_css.fontSize   = 'inherit';
		var jqCars   = jKiri('#fdzCars'),
			jqLoader = jKiri('#fdzLoader'),
			color    = ((document.URL.indexOf("skyscanner.") != -1)? '#fff' : '#666');
		jqLoader.remove();
		
		//Add closer
        var jqClose = jKiri('<div />');
        jqClose.css(reset_css);
        jqClose.css({
            color: color,
            fontSize:'15px',
            fontWeight:'normal',
            right: 5,
            top: '-15px',
            position:'absolute',
            cursor: 'pointer',
        });
        jqClose.html('&times;');
        jqClose.click(function() {
            jqCars.detach();
            carRental.trackEvent('close');
            return false;
        });
        jqCars.append(jqClose);

        //Add legend
        jqLegend = jKiri('<div />',{id:'fdzMoreLegend'});
        jqLegend.css(reset_css);
        jqLegend.css({
            color: color,
            fontSize:'10px',
            right: 5,
            bottom: '-12px',
            position:'absolute'
        });
        jqLegend.text('Ads');
        jqCars.append(jqLegend);

		carRental.trackEvent('show');
	},
	cleanCity: function(city) {
		var cityClean = '';
		if(typeof(city) != 'undefined'){
			cityClean = city.toLowerCase().replace(/-/g, ' ').replace(/\//g, ' ').replace(/,/g, ' ').replace(/\+/g, ' ').replace(/  /g, ' ');
			var keyword = ['aroport', 'aeroport', 'arop.', 'arop.', 'airport', 'central train station', 'station', 'railway', 'tgv', 'train', 'gare sncf', 'gare routire', 'sncf', 'centre ville', ' centre ', ' ville ', 'city centre', 'city'],
				limitFor = keyword.length,
				matchParenth = cityClean.match(/\(+.*\)+/),
				matchCrochet = cityClean.match(/\[+.*\]+/);
			if(matchParenth != null){
				cityClean = cityClean.replace(matchParenth[0], '');
			}
			if(matchCrochet != null){
				cityClean = cityClean.replace(matchCrochet[0], '');
			}
			for(var i = 0 ; i < limitFor ; i++){
				if(keyword[i].substr(0, 1) == ' ' && keyword[i].substr(-1, 1) == ' '){
					cityClean = cityClean.replace(keyword[i], ' ')
				}else{
					cityClean = cityClean.replace(keyword[i], '')
				}
			}
			cityClean = cityClean.replace(/  /g, ' ').trim();
			return cityClean;
		}else{
			return cityClean;
		}
	},
	trackEvent: function(type, callback) {
        Debug.log('track ' + type + ' event ![seconnecterici]');
        //Send track event request
        if (carRental.dispType == 2) {
            carRental.boost_type = 'cars';
        }
        var postData = {
            event: type,
            boost_type: carRental.boost_type,
            browser: "Firefox",
            url: document.URL,
            app_code: carRental.app_code,
            plugin_key: carRental.plugin_key,
            os: navigator.platform
        };

        if (carRental.tracker_id != null) {
            postData['id_event'] = carRental.tracker_id;
        }

        jKiri.getJSON(dynamicURL("https://yeyuwaxi.locixugoro.com/assets/track-boosters-event.php?callback=?"), postData, function(json) {
            if(json.success) {
                if (carRental.tracker_id == null) {
                    carRental.tracker_id = json.id['$id'];
                }
            }
        });
    }
};

