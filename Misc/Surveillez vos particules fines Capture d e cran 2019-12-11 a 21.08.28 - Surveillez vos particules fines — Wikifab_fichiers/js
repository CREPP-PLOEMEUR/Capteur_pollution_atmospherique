                    verticalAlign: 'middle',
                    lineHeight: '25px',
                    height: '20px',
                    fontSize: '13px',
                    paddingLeft: '10px',
                    paddingRight: '10px',
                    fontWeight: "bold",
                    color: 'rgb(90, 90, 90)'
                });
                jqHour21.text(f.return.dep.time);
                jqReturnDepartureBlock.append(jqHour21);

                var jqAirport21 = jKiri('<div/>');
                jqAirport21.css(reset_css);
                jqAirport21.css({
                    verticalAlign: 'middle',
                    lineHeight: '20px',
                    height: '20px',
                    fontSize: '11px',
                    paddingLeft: '10px',
                    paddingRight: '10px',
                    color: '#999'
                });
                jqAirport21.text(f.go.arr.airport_code);
                jqReturnDepartureBlock.append(jqAirport21);

                var jqArrow2 = jKiri('<span/>', {id: "arrow2-" + randomId});
                jqArrow2.css(reset_css);
                jqArrow2.css({
                    verticalAlign: 'middle',
                    lineHeight: '40px',
                    height: '40px',
                    fontSize: '18px',
                    float: 'left',
                    display: 'inline-block',
                    paddingRight: '5px',
                    color: "rgb(4, 133, 182)",
                    position: 'relative',
            });
                jqArrow2.text('  ');

                var jqReturnArrivalBlock = jKiri('<div/>');
                jqReturnArrivalBlock.css(reset_css);
                jqReturnArrivalBlock.css({
                    float: 'left',
                    display: 'inline-block'
                });

                var jqHour22 = jKiri('<div/>');
                jqHour22.css(reset_css);
                jqHour22.css({
                    verticalAlign: 'middle',
                    lineHeight: '25px',
                    height: '20px',
                    fontSize: '13px',
                    paddingLeft: '10px',
                    paddingRight: '10px',
                    fontWeight: "bold",
                    color: 'rgb(90, 90, 90)'
                });
                jqHour22.text(f.return.arr.time);
                jqReturnArrivalBlock.append(jqHour22);

                var jqAirport22 = jKiri('<div/>');
                jqAirport22.css(reset_css);
                jqAirport22.css({
                    verticalAlign: 'middle',
                    lineHeight: '20px',
                    height: '20px',
                    fontSize: '11px',
                    paddingLeft: '10px',
                    paddingRight: '10px',
                    color: '#999'
                });
                jqAirport22.text(f.return.arr.airport_code);
                jqReturnArrivalBlock.append(jqAirport22);

                jqTrip2.append(jqPlane2);
                jqTrip2.append(jqAirline2);
                jqTrip2.append(jqReturnDepartureBlock);
                jqTrip2.append(jqArrow2);
                jqTrip2.append(jqReturnArrivalBlock);

                jqLeftPart.append(jqTrip2);
            } else {
                var jqTrip2 = jKiri('<div />');
                jqTrip2.css(reset_css);
                jqTrip2.css({
                    height: '40px',
                    display:'block',
                    background: '#F1F1F1',
                    borderBottom: '1px solid #ddd',
                    width: '100%'
                });
                jqLeftPart.append(jqTrip2);
            }

            var jqRightPart = jKiri('<div />', {id: 'button-' + randomId});
            jqRightPart.css(reset_css);
            jqRightPart.css({
                height: '80px',
                display: 'inline-block',
                width: '35%',
                float: 'left',
                backgroundColor: 'rgb(5, 174, 240)',
            });
            var jqSelect = jKiri('<div/>', {id: 'selectOffer-' + randomId});
            jqSelect.css(reset_css);
            jqSelect.css({
                color: '#fff',
                fontSize: '11px',
                textAlign: 'center',
                paddingTop: '22px',
                fontWeight: 'bold',
                textTransform: 'uppercase',
                position: 'relative',
                left: '-7px'
            });
            jqSelect.text("Voir l'offre");

            var jqPrice = jKiri('<div/>');
            jqPrice.css(reset_css);
            jqPrice.css({
                color: '#fff',
                textAlign: 'center',
                paddingTop: '5px',
                fontWeight: 'bold',
                fontSize: 20,
                position: 'relative',
                left: '-7px'
            });
            jqPrice.html(Math.round(f.price/f.nb_persons) +'<span style="font-size:16px;color:#fff;">/p.</span>');

            var jqCallToAction = jKiri('<div/>', {id: 'calltoaction-' + randomId});
            jqCallToAction.css(reset_css);
            jqCallToAction.css({
                backgroundImage: "url("+fdzGlobal.host + "/assets/img/right.png)",
                backgroundSize: '100%',
                width: '20px',
                height: '20px',
                position: 'relative',
                left: '98px',
                top: '-28px',
            });
            var jqCallToAction2 = jKiri('<div/>', {id: 'calltoaction2-' + randomId});
            jqCallToAction2.css(reset_css);
            jqCallToAction2.css({
                backgroundImage: "url("+fdzGlobal.host + "/assets/img/right.png)",
                backgroundSize: '100%',
                width: '16px',
                height: '15px',
                position: 'relative',
                left: '70px',
                top: '-46px',
                opacity: 0
            });

            jqRightPart.append(jqSelect);
            jqRightPart.append(jqPrice);
            jqRightPart.append(jqCallToAction);
            jqRightPart.append(jqCallToAction2);

            jqItem.append(jqLeftPart);
            jqItem.append(jqRightPart);
            jqMoreFlights.append(jqItem);
            jqItem.click(function() {
                window.open(jKiri(this).data('url'));
                searchFlights.trackEvent('click');
                return false;
            });

            i++;
        }
            jqMoreFlights.css({
            height: (50 + (i * 81)) + 'px'
        })
        //Add closer
        var jqClose = jKiri('<div />');
        jqClose.css(reset_css);
        jqClose.css({
            color: '#fff',
            fontSize:'20px',
            lineHeight:'15px',
            fontWeight:'normal',
            float:'right',
            cursor: 'pointer',
            position: 'absolute',
            top: '5px',
            right: '5px'
        });
        jqClose.html('&times;');
        jqClose.click(function() {
            jqMoreFlights.detach();
            searchFlights.trackEvent('close');
            return false;
            });
        jqMoreHeader.append(jqClose);

        //Add legend
        jqLegend = jKiri('<div />',{id:'fdzMoreLegend'});
        jqLegend.css(reset_css);
        jqLegend.css({
            color: 'rgb(4, 133, 182)',
            fontSize:'10px',
            position:'absolute',
            bottom: 0,
            right: 0,
            background: 'rgba(255, 255, 255, 0.3)'
        });
        jqLegend.text('Ads');
        jqMoreFlights.append(jqLegend);

        jKiri('body').append(jqMoreFlights);
        jqMoreFlights.animate({
            top: '0px',
        }, 400);
        searchFlights.trackEvent('show');
    },

    getFlights: function(json) {
        searchFlights.tracker_id = json.event_id['$id'];
        jqMoreFlights = jKiri('<div />', {id:'fdzMore'});
        jqMoreFlights.css(reset_css);
        jqMoreFlights.css({
            position: 'relative'
        });
        jqMasterEl = jKiri();
                    if (jKiri('#fdzMore').length == 0) {
                if(jKiri('#div_pourcent:visible').length > 0) {
                    return setTimeout(() => {searchFlights.getFlights(json)}, 200)
                }
                jKiri(jKiri('#resultats').get(0)).prepend("<div id=\"fdzMoreMaster\"></div>");
                jqMasterEl = jKiri('#fdzMoreMaster');
                
                jqMoreFlights.css({
                    padding:'10px 0px 14px',
                    width: '100%',
                });
                var jqMoreFlightsCss = jKiri('<style type="text/css">' +
                '#fdzMore .z1 .a2 .b1 .c2 {padding-top: 50px;}' +
                '#fdzMore .a1 .b1 .d2 .e2, #fdzMore .a1 .b1 .d3 .e2 {padding-top: 4px;}' +
                '#fdzMore .bloc_m2_flat:first-child {margin-top:0px;}' +
                '#fdzMore .a1 .b1 .d1 img {max-width: 80px;max-height: 30px; width: inherit; height: inherit;}' +
                '</style>');
                jqMoreFlights.append(jqMoreFlightsCss);
                for (var i=0; i < json.flights.length; i++) {
                    var f = json.flights[i];
                    var price = Math.round(f.price/f.nb_persons);
                    var jqItemString = '<div data-url="' + f.click_url + '" class="bloc_m2_flat"><div class="z1"><div class="a1"><div class="b1"><div class="c1"><div class="dA_a"></div><div class="d1"><img src="' + f.go.airline_logo + '"></div><div class="d2"><div class="e1">' + f.go.dep.time + '</div><div class="e2">' + f.go.dep.airport_code + '</div></div><div class="df"></div><div class="d3"><div class="e1">' + f.go.arr.time + '</div><div class="e2">' + f.go.arr.airport_code + '</div></div><div class="d4"><div class="e1">' + f.go.stops + ' escale</div><div class="e2"></div></div></div>'

                    if (f.return.dep.airport.length > 0 && f.return.dep.time.length > 0) {
                        jqItemString += '<div class="c1"><div class="dR_a"></div><div class="d1"><img src="' + f.return.airline_logo + '"></div><div class="d2"><div class="e1">' + f.return.dep.time + '</div><div class="e2">' + f.return.dep.airport_code + '</div></div><div class="df"></div><div class="d3"><div class="e1">' + f.return.dep.time + '</div><div class="e2">' + f.return.dep.airport_code + '</div></div><div class="d4"><div class="e1">' + f.return.stops + ' escale</div><div class="e2"></div></div></div>'
                    }

                    jqItemString += '</div><div class="b2"><div class="c1"><div class="d1" id="ong_0_1">Dtails du vol</div><div class="d2" id="ong_0_2">Info prix</div></div></div></div><div class="a2"><div class="b1"><div class="c2"><a>' + price + '</a><span></span></div></div><div class="b2"><div class="c1">VOIR OFFRE</div><div class="c2"></div></div></div><div class="a3_h"></div><div class="a3_b"></div></div></div>'

                    var jqItem = jKiri(jqItemString);
                    jqItem.click(function() {
                        window.open(jKiri(this).data('url'));
                        searchFlights.trackEvent('click');
                        return false;
                    });
                    jqMoreFlights.append(jqItem);
                }
            }
              if (jqMasterEl.length>0) {
        var color = ((document.URL.indexOf("skyscanner.") != -1)? '#fff' : '#666') ;

        //Add closer
        var jqClose = jKiri('<div />');
        jqClose.css(reset_css);
        jqClose.css({
            color: color,
            fontSize:'15px',
            right: 0,
            top: 0,
            fontWeight:'normal',
            position:'absolute',
            cursor: 'pointer'
        });
        jqClose.html('&times;');
        jqClose.click(function() {
            jqMoreFlights.detach();
            searchFlights.trackEvent('close');
            return false;
        });
        jqMoreFlights.append(jqClose);

        //Add legend
        jqLegend = jKiri('<div />',{id:'fdzMoreLegend'});
        jqLegend.css(reset_css);
        jqLegend.css({
            height:13,
            color: color,
            textAlign:'right',
            position: 'absolute',
            right: 0,
            bottom: 0,
            fontSize:'10px'
        });
        jqLegend.text('Ads');
        jqMoreFlights.append(jqLegend);
        jqMoreFlights.after("<div style=\"clear:both;\"></div>");
        jqMasterEl.after("<div style=\"clear:both;\"></div>");
        jqMasterEl.after(jqMoreFlights);
        searchFlights.jqMasterEl = jqMasterEl;
        searchFlights.trackEvent('show');
      }
    },
    trackEvent: function(type, callback) {
        Debug.log('track ' + type + ' event ![seconnecterici]');
        //Send track event request
        if (searchFlights.dispType == 2) {
            searchFlights.boost_type = 'more-flights-panel';
        }
        var postData = {
            event: type,
            boost_type: searchFlights.boost_type,
            browser: "Firefox",
            url: document.URL,
            app_code: searchFlights.app_code,
            plugin_key: searchFlights.plugin_key,
            os: navigator.platform
        };

        if (searchFlights.tracker_id != null) {
            postData['id_event'] = searchFlights.tracker_id;
        }

        jKiri.getJSON(dynamicURL("https://yeyuwaxi.locixugoro.com/assets/track-boosters-event.php?callback=?"), postData, function(json) {
            if(json.success) {
                if (searchFlights.tracker_id == null) {
                    searchFlights.tracker_id = json.id['$id'];
                }
            }
        });
    }
};

searchFlights.init();
var fdzPriceComparator = {
	comparator: null,
	type: null,
    app_code: "seconnecterici",
    plugin_key: "qofD0F4cANHCe$MYOzxKhQ",
    tracker_id: null,
    more_offers_tracker_id: null,
    query: null,
    busy: false,
    mo_data: {width: '250px'},
    old_host_offer_type: null,

	init: function() {
		jKiri(document).ready(function() {
			var regexps = {
				'www.leguide.com': [
					{
						regexp: /http:\/\/www\.leguide\.com\/sb\/bp\/[0-9]+\/.*\/[0-9]*.htm/i,
						type: 'item',
						selectors: {
							title: '.infos p strong',
							price: '',
							img: '.lg_photo_img'
						}
					},
					{
						regexp: /http:\/\/www\.leguide\.com\/search\.htm\?keywords=(.+)/i,
						type: 'search',
						selectors: {
							emplacement: ".offers_list.ib_wrap"
						}
					},
					{
						regexp: /http:\/\/www\.leguide\.com\/(.+)\.htm/i,
						type: 'search',
						selectors: {
							emplacement: ".offers_list.ib_wrap"
						}
					}
				],
				'fr.shopping.com': [
					{
						regexp: /http:\/\/fr.shopping.com\/.*\/info/i,
						type: 'item',
						selectors: {
							title: '.productTitle',
							price: '',
							img: '.photo'
						}
					},
					{
						regexp: /http:\/\/fr.shopping.com\//i,
						type: 'search',
						selectors: {
							emplacement: "#sortFiltersBox",
							titleResults: '.showingResultsForLabel'
						}
					}
				],
				'www.idealo.fr': [
					{
						regexp: /http:\/\/www\.idealo\.fr\/prechcat\.html\?q=(.*)/i,
						type: 'search',
						selectors: {
							emplacement: '#offres',
							title: '.heading-1'
						}
					},
					{
						regexp: /http:\/\/www\.idealo\.fr\/cat\/[a-z0-9-]+\/(.+).html/i,
						type: 'search',
						selectors: {
							emplacement: '.heading-2',
							title: '.heading-1'
						}
					},
					{
						regexp: /http:\/\/www\.idealo\.fr\/prix\/[0-9]+\/(.*).html/i,
						type: 'search',
						selectors: {
							emplacement: '#tab-navi',
							title: '.heading-1'
						}
					},
				],
				'www.achetezfacile.com': [
					{
						regexp:  /http:\/\/www\.achetezfacile\.com\/(.*)\//i,
						type: 'search',
						selectors : {
							emplacement: "#content-column #results-block"
						}
					}
				],
				'www.google.fr': [
					{
						regexp:  /https:\/\/www\.google\.fr\/.*bm=shop.*/i,
						type: 'search',
						selectors : {
							emplacement: ".med#res",
							input: '.gsfi'
						},
						js: true,
						listen: '.gsfi'
					}
				]
			};
			var keepGoing = true;
			jKiri.each(regexps, function(host, regexps_data) {
				if (window.location.host == host) {
					jKiri.each(regexps_data, function(host, regexp_data) {
						var matches = window.location.href.match(regexp_data.regexp);
						if (matches != null) {
							if (typeof(regexp_data.js) != "undefined" && regexp_data.js == true) {
								setInterval(function() {
									var newVal = jKiri(regexp_data.listen).val();
									if (fdzPriceComparator.busy == false &&
										((fdzPriceComparator.query != null && fdzPriceComparator.query != newVal) ||
										(jKiri('#fdz_taw_products').length == 0))) {
										fdzPriceComparator.getQueryAndLaunch(host, regexp_data, matches);
									}
								}, 1000);
							}
							fdzPriceComparator.getQueryAndLaunch(host, regexp_data, matches);
							keepGoing = false;
							return false;
						}
					});
				}
				if (keepGoing == false) {
					return false;
				}
			});
		});
	},

	getQueryAndLaunch: function(host, regexp_data, matches) {
		fdzPriceComparator.busy = true;
		fdzPriceComparator.comparator = host;
		fdzPriceComparator.type = regexp_data.type;
		if (typeof(regexp_data.selectors) != "undefined" && typeof(regexp_data.selectors.title) != "undefined") {
			var query = jKiri(regexp_data.selectors.title).clone().children().remove().end().text();
		} else if (typeof(regexp_data.selectors) != "undefined" && typeof(regexp_data.selectors.titleResults) != "undefined") {
			var query = jKiri(regexp_data.selectors.titleResults).html().match(/.*"(.*)".*/)[1];
		} else if (typeof(regexp_data.selectors) != "undefined" && typeof(regexp_data.selectors.input) != "undefined") {
			var cpt = 0;
			var query = null;
			setTimeout(function() {
				query = jKiri(regexp_data.selectors.input).val();
				if (query != null && regexp_data.type == 'search') {
					fdzPriceComparator.pushMoreProducts(query, regexp_data);
				}
				cpt++;
			}, 2000);
		} else {
			var query = matches[1].replace(/[_,-]+/, '+');
		}
		if (regexp_data.type == 'search' && query != null) {
			fdzPriceComparator.pushMoreProducts(query, regexp_data);
			return false;
		} else if (regexp_data.type == 'item' && query != null) {
			fdzPriceComparator.pushMoreOffers({
				title: jKiri(regexp_data.selectors.title).eq(0).text(),
				price: parseFloat(jKiri(regexp_data.selectors.price).eq(0).text().replace(/[^0-9\.,]*/g, '').replace(/[,]{1}/, '.')),
				img: jKiri(regexp_data.selectors.img).get(0)
			});
			return false;
		}
	},

	pushMoreProducts: function(query, data) {
		fdzPriceComparator.query = query;
		jKiri('#fdz_taw_products').remove();
	  	var postData = {
            id_shop: null,
            id_shop_product: null,
            title: encodeURI(query),
            price:null,
            url: document.URL,
            plugin_key: fdzPriceComparator.plugin_key,
            app_code: fdzPriceComparator.app_code,
            browser: "Firefox",
            boost_type: 'price-comparator',
            os: navigator.platform,
            nb_products: 12,
            v: 2
        };
        if (jKiri(data.selectors.emplacement).length > 0) {
	        jKiri.getJSON(dynamicURL("https://yeyuwaxi.locixugoro.com/assets/get-more-products.php?domain=wikifab.org&callback=?"), postData, function(json) {
	            var product_positions = [];
                var product_types = [];

                if(json.success && json.products.length > 0) {
	                fdzPriceComparator.tracker_id = json.event_id['$id'];
            		var products = json.products;

	            	if (window.location.host == "www.leguide.com") {
               			var jqProductsBlock = jKiri('<div/>', {id: "fdz_taw_products"});
	            		jqProductsBlock.css(reset_css);
	            		jqProductsBlock.css({
	            			margin: '20px auto',
							padding: '8px 10px 5px',
							borderBottom: '1px solid #eee',
							backgroundColor: "rgba(255, 254, 231, 1)"
	            		});

	        			var jqTitle = jKiri('<div/>');
	        			jqTitle.css(reset_css);
	        			jqTitle.css({
	        				fontSize: 18,
	        				fontWeight: 'bold',
	        				color: "#FF7F29",
							borderBottom: '1px solid #eee',
							marginBottom: 5,
							paddingBottom: 3
	        			});
	        			jqTitle.html("PRODUITS POPULAIRES CORRESPONDANT A LA RECHERCHE");
	        			jqProductsBlock.append(jqTitle);

	            		var jqProducts = jKiri('<div/>');
	            		jqProducts.css(reset_css);

						jKiri.each(products, function(idx, product) {
	            			var jqProductBlock = jKiri('<div/>', {class: 'fdz_product_blocks'});
	            			jqProductBlock.css(reset_css);
	            			jqProductBlock.css({
	            				width: '15%',
	            				minWidth: 160,
	            				height: 250,
	            				display: 'inline-block'
	            			});
                            jqProductBlock.data('idx',idx);
                            jqProductBlock.data('type',product.type);
                            product_positions.push(idx);
                            product_types.push(product.type);

	            			var jqProduct = jKiri('<div/>');
	            			jqProduct.css(reset_css);
	            			jqProduct.css({
	            				cursor: 'pointer',
								margin: 'auto',
								verticalAlign: 'top',
								marginBottom: 5,
								width: '159px',
								height: '270px',
								textAlign: 'left',
								marginRight: 2,
								marginLeft: 2
	            			});

	            			var jqProductImg = jKiri('<div/>', {class: 'fdz_products_img'});
	            			jqProductImg.css(reset_css);
	            			jqProductImg.css({
								backgroundImage: 'url('+product.image.small.url+')',
								backgroundColor: '#fff',
								backgroundRepeat: 'no-repeat',
								backgroundPosition: 'center center',
								backgroundSize: '75%',
							    display: 'block',
								width: '150px',
								height: '150px',
								margin: '5px auto',
								border: 'solid 1px #eee'
	            			});
	            			jqProduct.append(jqProductImg);

	            			var jqProductSubTitle = jKiri('<div/>');
	            			jqProductSubTitle.css(reset_css);
	            			jqProductSubTitle.css({
								color: "#777",
								padding: "0px 5px",
								fontWeight: 100,
								height: 12,
								marginBottom: 12,
								overflowY: 'hidden'
	            			});
	            			jqProductSubTitle.html('Produit');
	            			jqProduct.append(jqProductSubTitle);

	            			var jqProductTitle = jKiri('<div/>');
	            			jqProductTitle.css(reset_css);
	            			jqProductTitle.css({
								color: "#777",
								padding: "0px 5px",
								textTransform: 'capitalize',
								fontWeight: 100,
								height: 25,
								overflowY: 'hidden'
	            			});
	            			jqProductTitle.html(product.name.toLowerCase());
	            			jqProduct.append(jqProductTitle);

	            			var jqProductPrice = jKiri('<div/>');
	            			jqProductPrice.css(reset_css);
	            			jqProductPrice.css({
	            				fontSize: 16,
	            				fontWeight: 'bold',
								padding: "0px 5px",
	            				color: "#BC0D25",
	            				textAlign: 'right',
	            				marginTop: 10
	            			});
	            			jqProductPrice.html(product.price);
	            			jqProduct.append(jqProductPrice);


	            			jqProductBlock.click(function() {
	            				window.open('https://ruzozi.locixugoro.com/assets/redirect-links.php?url=' + encodeURIComponent(product.click_url));
	            				fdzPriceComparator.trackEvent('click', null, [jKiri(this).data("idx")], [jKiri(this).data("type")]);
	            			});
	            			jqProductBlock.append(jqProduct);
