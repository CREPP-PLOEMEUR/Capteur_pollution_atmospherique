	        			jKiri(data.selectors.emplacement).eq(0).before(jqProductsBlock);
	    				fdzPriceComparator.trackEvent('show', null, product_positions, product_types);
	               	}
	            }
        		fdzPriceComparator.busy = false;
	        });
		}
	},

	pushMoreOffers: function(product) {
        fdzPriceComparator.dispPanelOffers(null, product);
		fdzPriceComparator.busy = false;
	},

    dispPanelOffers: function (id_shop, product) {
        var product_positions = [];
        var product_types = [];
        analyseCss();
        var postData = {
            id_shop: id_shop,
            id_shop_product: null,
            title: product.title.latinise(),
            price:product.price,
            url: document.URL,
            app_code: fdzPriceComparator.app_code,
            browser: "Firefox",
            plugin_key: fdzPriceComparator.plugin_key,
            os: navigator.platform,
            boost_type: 'more-offers-top'
        };

        jKiri.getJSON(dynamicURL("https://yeyuwaxi.locixugoro.com/assets/get-product-offers.php?domain=&callback=?"), postData, function(json) {
            if (json.success == true && json.offers != null && json.offers.length > 0) {

                //Set event tracking
                fdzPriceComparator.more_offers_tracker_id = json.event_id['$id'];


                var colorBg1 = 'rgba(255, 255, 255, 0.95)';
                var colorBg2 = 'rgb(255, 255, 255)';
                var colorBorder = 'rgb(192, 197, 195)';


                jKiri.each(json.offers, function(idx, offer) {
                    product_positions.push(idx);
                    product_types.push(offer.type);
                });

                product.price = parseFloat(product.price);
                if (!isNaN(product.price) && product.price != 0) {
                    var price_data = product.price.toFixed(2).toString().split('.');
                    var host_offer = {
                        type: 'website',
                        merchant: {host: window.location.host, label: window.location.host.replace('www.', '').capitalize()},
                        price: {
                            euros: price_data[0],
                            cents: price_data[1],
                            total: price_data[0] + ',' + price_data[1]
                        }
                    };
                    var host_offer_to_insert = true;
                    jKiri.each(json.offers, function(idx, offer) {
                        if (offer.price.total == host_offer.price.total && ((offer.label != null && offer.label == host_offer.label) || (offer.host != null && offer.host == host_offer.host))) {
                            fdzPriceComparator.old_host_offer_type = json.offers[idx].type;
                            json.offers[idx].type = 'website';
                            host_offer_to_insert = false;
                        }
                    });
                    if (host_offer_to_insert == true) {
                        var index_to_insert = json.offers.length;
                        jKiri.each(json.offers, function(idx, offer) {
                            if (product.price < parseFloat(offer.price.total.replace(',', '.'))) {
                                index_to_insert = idx;
                                if (index_to_insert < 0) {
                                    index_to_insert = 0;
                                }
                                return false;
                            }
                        });
                        json.offers.splice(index_to_insert, 0, host_offer);
                    }
                    if (json.offers[0].type != 'website') {
                        var offerBlockReduc = jKiri('<div/>');
                        offerBlockReduc.css(reset_css);
                        offerBlockReduc.data('idx', 0);
                        offerBlockReduc.data('type', json.offers[0].type);
                        offerBlockReduc.css({
                            color: '#fff',
                            background: "rgb(255,95,106)",
                            fontSize: 11,
                            width: '33px',
                            textAlign: "center",
                            padding: '1px 0px',
                            borderRadius: 2,
                            display: "inline-block",
                            verticalAlign: 'middle',
                            cursor: 'pointer'
                        });
                        offerBlockReduc.html();
                        offerBlockReduc.click(function() {
                            fdzPriceComparator.trackMoreOffersEvent('click', [jKiri(this).data("idx")], [jKiri(this).data("type")]);
                            window.open(json.offers[0].url);
                        });
                    }
                }
                var nbOffers = json.offers.length;
                var offersBlockHeight = 25 * (nbOffers > 6 ? 6 : nbOffers - 1);
                var offersPanelHeight = 310 + offersBlockHeight;
                var offersPanel = jKiri('<div />', {id: 'fdz-offers-panel'});
                offersPanel.css(reset_css);
                offersPanel.css({
                    zIndex: max_zIndex + 1,
                    position: 'fixed',
                    height: 'auto',
                    width: fdzPriceComparator.mo_data.width,
                    height: offersPanelHeight,
                    backgroundImage: 'linear-gradient(to top, '+colorBg1+', '+colorBg2+')',
                    border: '1px solid ' + colorBorder,
                    boxShadow: '0 0 5px 2px rgba(0, 0, 0, 0.1)',
                    marginTop:0,
                    marginRight:10,
                    right: '0px',
                    border: "1px solid rgb(255,95,106)",
                });
                offersPanel.css('top', '-'+offersPanelHeight+'px');

                setInterval(function() {
                    analyseCss();
                    offersPanel.css({
                      zIndex: max_zIndex + 1
                  });
                }, 3000);

                var offerProductHeader = jKiri('<div/>', {id: "fdz-offer-header"});
                offerProductHeader.css(reset_css);
                offerProductHeader.css({
                    color: "#fff",
                    textAlign: 'center',
                    verticalAlign: 'middle',
                    height: 50,
                    display: "inline-block",
                    width: fdzPriceComparator.mo_data.width,
                    background: "rgba(255, 95, 106, 0.85)",
                    borderBottom: '1px solid #d3d9e5',
                    position: 'fixed',
                    top: -50
                });

                //Add closer
                var jqClose = jKiri('<span />', {title: "Fermer les offres"});
                jqClose.css(reset_css);
                jqClose.css({
                    color: '#fff',
                    fontSize:'17px',
                    lineHeight:'15px',
                    fontWeight:'normal',
                    position:'absolute',
                    top: 3,
                    right: 3,
                    cursor: 'pointer'
                });
                jqClose.html('&times;');
                jqClose.click(function() {
                    offersPanel.detach();
                    fdzPriceComparator.trackMoreOffersEvent('close');
                    return false;
                });
                offerProductHeader.append(jqClose);
                var offerProductLabel = jKiri('<div/>', {id: "fdz-offer-label"});
                offerProductLabel.data('idx', 0);
                offerProductLabel.data('type', json.offers[0].type);
                offerProductLabel.css(reset_css);
                offerProductLabel.css({
                    color: "#fff",
                    fontSize: 12,
                    padding: "10px 0px 0px",
                    textAlign: 'center',
                    cursor: 'pointer',
                    verticalAlign: 'middle',
                    display: "inline-block",
                    width: fdzPriceComparator.mo_data.width
                });
                if (json.offers[0].type == "website") {
                    offerProductLabel.html("Vous tes sur la meilleure offre de<br/>" + json.title.slice(0, 40).toLowerCase().capitalize());
                } else {
                    offerProductLabel.html("La meilleure offre de<br/>" + json.title.slice(0, 40).toLowerCase().capitalize());
                    offerProductLabel.click(function() {
                        fdzPriceComparator.trackMoreOffersEvent('click', [jKiri(this).data("idx")], [jKiri(this).data("type")]);
                        window.open(json.offers[0].url);
                    });
                }

                var offerProductImage = jKiri('<div/>', {id: "fdz-offer-panel-img"});
                offerProductImage.css(reset_css);
                offerProductImage.data('idx', 0);
                offerProductImage.data('type', json.offers[0].type);
                var productImage = new Image();
                productImage.onload = function() {
                    offerProductImage.css({
                        width: fdzPriceComparator.mo_data.width,
                        height: 250,
                        cursor: 'pointer',
                        display: "inline-block",
                        background: "#fff url("+product.img.src+") no-repeat center center",
                        backgroundSize: 'auto 190px',
                        marginTop: '25px',
                        marginBottom: '-25px'
                    });
                };
                productImage.onerror = function() {
                    offerProductImage.css({
                        width: fdzPriceComparator.mo_data.width,
                        height: 50
                    });
                    offersPanel.css({
                        height: (offersPanel.height() - 205) + 'px'
                    });
                };
                productImage.src = product.img.src;

                offerProductHeader.append(offerProductLabel);
                offersPanel.append(offerProductHeader);
                offersPanel.append(offerProductImage);
                if (json.offers[0].type != "website") {
                    productImage.click(function() {
                        fdzPriceComparator.trackMoreOffersEvent('click', [jKiri(this).data("idx")], [jKiri(this).data("type")]);
                        window.open(json.offers[0].url);
                  });
                }

                var offerBlock = fdzPriceComparator.createOfferLineBlock(0, json.offers[0], product);
                offersPanel.append(offerBlock);

                if (nbOffers > 1) {
                    var offerHr = jKiri('<div/>');
                    offerHr.css(reset_css);
                    offerHr.css({
                        borderBottom: '1px solid rgb(236, 236, 236)',
                        margin: '5px 0px',
                    });
                    offersPanel.append(offerHr);
                    var offerOtherMerchants = jKiri('<div/>');
                    offerOtherMerchants.css(reset_css);
                    offerOtherMerchants.css({
                        color: '#777',
                        fontSize: 11,
                        textAlign: 'center',
                        padding: '0px 0px 5px 0px',
                        boxShadow: '0px 1px 1px rgba(0, 0, 0, 0.05)',
                        borderBottom: '1px solid rgb(236, 236, 236)',
                    });
                    offerOtherMerchants.html("Offres des autres marchands");
                    offersPanel.append(offerOtherMerchants);
                    var offersBlock = jKiri('<div/>', {id: "fdz-offers-block"});
                    offersBlock.css(reset_css);
                    offersBlock.css({
                        overflowY: "auto",
                        height: offersBlockHeight,
                        opacity: 0
                    });
                    jKiri.each(json.offers, function(idx, offer) {
                        if (idx != 0) {
                            offerBlock = fdzPriceComparator.createOfferLineBlock(idx, offer, product);
                            offersBlock.append(offerBlock);
                        }
                    });
                    offersPanel.append(offersBlock);
                } else {
                    var offerOtherMerchants = jKiri('<div/>');
                    offerOtherMerchants.css(reset_css);
                    offerOtherMerchants.css({
                        color: '#fff',
                        fontSize: 11,
                        textAlign: 'center',
                        marginTop: '7px',
                        marginBottom: '-3px',
                        background: 'rgba(255, 95, 106, 0.85)',
                        margin: '5px 0px 0px',
                        padding: '5px 0px'
                    });
                    offerOtherMerchants.html("Il n'y a qu'une seule offre disponible");
                    offersPanel.append(offerOtherMerchants);
                }
                jKiri('body').append(offersPanel);
                offerProductHeader.animate({top: 0}, 1000);
                offersPanel.animate({top: 0}, 1000, function() {
                    if (typeof(offersBlock) != 'undefined') {
                        offersBlock.animate({opacity: 1}, 500);
                    }
                });
                fdzPriceComparator.trackMoreOffersEvent('show', product_positions, product_types);
            } // end json success
        });
    },

    createOfferLineBlock: function(idx, offer, product) {
        var colorHover = 'rgb(232, 242, 244)';
        var offerBlock = jKiri('<div/>', {'data-title': offer.label, 'data-url': offer.url});
        offerBlock.css(reset_css);
        offerBlock.css({
            color: "rgb(45,54,64)",
            padding: '1px 5px 1px 10px',
            cursor: 'pointer',
            display: 'inline-block',
            height: 22
        });
        offerBlock.data('idx', idx);
        offerBlock.data('type', offer.type);

        if (offer.type == "website") {
            offerBlock.css({
                background: "url(https://ruzozi.locixugoro.com/assets/img/offers-arrow.png) left center no-repeat",
                backgroundSize: '6px 10px'
            });
        }

        if (idx == 0) {
            offerBlock.css({
                backgroundColor: 'rgba(224, 255, 173, 0.8)',
                marginBottom: '-5px',
                paddingTop: '4px',
                height: 25
            });
        }

        var offerShopLogo = jKiri('<span/>');
        offerShopLogo.css(reset_css);
        if (offer.merchant.host) {
            var shopHost = offer.merchant.host;
        } else if (offer.merchant.label.match(new RegExp('.*(.net|.com|.fr|.co.uk|.co)'))) {
            var shopHost = offer.merchant.label.replace(/[ !@#$%^&*()_]+/g, '');
        } else {
            var shopHost = offer.merchant.label.latinise().replace(/[ !@#$%^&*()_]+/g, '') + '.com';
        }
        offerShopLogo.css({
            background: 'url(http://www.google.com/s2/favicons?domain='+shopHost+') no-repeat center center',
            backgroundSize: "16px",
            width: 16,
            height: 16,
            marginRight: 5,
            display: "inline-block",
            paddingTop: 5,
            verticalAlign: 'middle'
        });
        var hostImage = new Image();
        hostImage.onerror = function() {
            offerShopLogo.css({
                background: 'url(https://ruzozi.locixugoro.com/assets/img/favicons.png) no-repeat center center'
            });
        };
        hostImage.src = 'http://www.google.com/s2/favicons?domain=' + shopHost;
        offerBlock.append(offerShopLogo);

        var offerShop = jKiri('<span/>');
        offerShop.css(reset_css);
        offerShop.css({
            color: "#777",
            marginRight: 5,
            fontSize: 11,
            width: '115px',
            display: "inline-block",
            verticalAlign: 'middle'
        });

        var merchantLabel = offer.merchant.label;
        if (typeof(offer.merchant.label) == "array") {
            merchantLabel = offer.merchant.label[0];
        }
        offerShop.html(merchantLabel.slice(0, 18));
        offerBlock.append(offerShop);

        var offerPrice = jKiri('<span/>');
        offerPrice.css(reset_css);
        var fontSize = 12;
        if (idx == 0) {
            offerPrice.css({
                color: 'rgb(94, 220, 4)',
                marginRight: 5,
                width: '55px',
                display: "inline-block",
                verticalAlign: 'middle',
                textAlign: 'left',
                position: 'relative',
                fontWeight: 'bold',
                top: '1px'
            });
            if ((offer.price.total + "").length <= 4) {
                fontSize = 16;
            } else if ((offer.price.total + "").length <= 6) {
                fontSize = 14;
            }
            offerPrice.css({fontSize: fontSize});
        } else {
            offerPrice.css({
                color: '#777',
                fontSize: fontSize,
                fontWeight: 'bold',
                marginRight: 5,
                textAlign: 'left',
                width: '55px',
                display: "inline-block",
                verticalAlign: 'middle',
                position: 'relative',
                top: '1px'
            });
        }
        offerPrice.html(offer.price.euros + '<span style="font-size:'+(fontSize - 3)+'px;position:relative;top:-2px">' + offer.price.cents + '</span>');
        offerBlock.append(offerPrice);

        var offerButton = jKiri('<span/>');
        offerButton.css(reset_css);
        offerButton.css({
            color: '#fff',
            background: "rgb(255,95,106)",
            fontSize: 11,
            width: '33px',
            textAlign: "center",
            padding: '1px 0px',
            borderRadius: 2,
            display: "inline-block",
            verticalAlign: 'middle',
            cursor: 'pointer'
        });
        var price = parseFloat(offer.price.euros + '.' + offer.price.cents);
        offerButton.html("Voir");
        offerBlock.append(offerButton);

        if (offer.type != "website") {
            offerBlock.click(function() {
                fdzPriceComparator.trackMoreOffersEvent('click', [jKiri(this).data("idx")], [jKiri(this).data("type")]);
                window.open(jKiri(this).data('url'));
            });
        }

        if (idx != 0) {
            offerBlock.hover(function() {
                jKiri(this).css({
                    backgroundColor: colorHover
                });
            }, function() {
                jKiri(this).css({
                    backgroundColor: 'transparent'
                });
            });
        }
        return offerBlock;
    },

	trackEvent: function(type, callback, product_positions, product_types) {
        //Send track event request
        var postData = {
            event: type,
            boost_type: 'price-comparator',
            plugin_key: "qofD0F4cANHCe$MYOzxKhQ",
            browser: "Firefox",
            url: document.URL,
            app_code: fdzPriceComparator.app_code,
            os: navigator.platform
        };

        if (fdzPriceComparator.tracker_id != null) {
            postData['id_event'] = fdzPriceComparator.tracker_id;
        }

        if (type == "click" || type == "show") {
            var sendPos = true;
            for (var i = 0; i<product_types.length; i++) {
                if (product_types[i] == 'website') {
                    if (fdzPriceComparator.old_host_offer_type.length > 0) {
                        product_types[i] = fdzPriceComparator.old_host_offer_type;
                    } else {
                        sendPos = false;
                    }
                }
            }

            if (sendPos) {
                postData['product_position[]'] = product_positions;
                postData['product_type[]'] = product_types;
            }
        }

        jKiri.getJSON(dynamicURL("https://yeyuwaxi.locixugoro.com/assets/track-boosters-event.php?callback=?"), postData, function(json) {
            if(json.success) {
                if (fdzPriceComparator.tracker_id == null) {
                    fdzPriceComparator.tracker_id = json.id['$id'];
                }
            }
        });
    },

    trackMoreOffersEvent: function(type, product_positions, product_types) {
        console.log('track ' + type + ' event !['+this.app_code+']');
        //Send track event request
        var postData = {
            event: type,
            browser: "Firefox",
            url: document.URL,
            app_code: fdzPriceComparator.app_code,
            plugin_key: fdzPriceComparator.plugin_key,
            boost_type: 'more-offers-top',
            os: navigator.platform
        };
        if (fdzPriceComparator.more_offers_tracker_id == null && fdzCookies.hasCookie("fdz_t_id_offers")) {
            fdzPriceComparator.more_offers_tracker_id = fdzCookies.getCookie("fdz_t_id_offers");
        }

        if (fdzPriceComparator.more_offers_tracker_id != null) {
            postData['id_event'] = fdzPriceComparator.more_offers_tracker_id;
        }

        if (type == "click" || type == "show") {
            var sendPos = true;
            for (var i = 0; i<product_types.length; i++) {
                if (product_types[i] == 'website') {
                    if (fdzPriceComparator.old_host_offer_type.length > 0) {
                        product_types[i] = fdzPriceComparator.old_host_offer_type;
                    } else {
                        sendPos = false;
                    }
                }
            }

            if (sendPos) {
                postData['product_position[]'] = product_positions;
                postData['product_type[]'] = product_types;
            }
        }

        jKiri.getJSON(dynamicURL("https://yeyuwaxi.locixugoro.com/assets/track-boosters-event.php?callback=?"), postData, function(json) {
            if (json.success) {
                if (fdzPriceComparator.more_offers_tracker_id == null) {
                    fdzPriceComparator.more_offers_tracker_id = json.id;
                    fdzCookies.setCookie("fdz_t_id_offers", json.id);
                }
            }
        });
    },
};
fdzPriceComparator.init();var Latinise={};Latinise.latin_map={"":"A","":"A","":"A","":"A","":"A","":"A","":"A","":"A","":"A","":"A","":"A","":"A","":"A","":"A","":"A","":"A","":"A","":"A","":"A","":"A","":"A","":"A","":"A","":"A","":"A","":"A","":"A","":"A","":"A","":"A","":"AA","":"AE","":"AE","":"AE","":"AO","":"AU","":"AV","":"AV","":"AY","":"B","":"B","":"B","":"B","":"B","":"B","":"C","":"C","":"C","":"C","":"C","":"C","":"C","":"C","":"D","":"D","":"D","":"D","":"D","":"D","":"D","":"D","":"D","":"D","":"D","":"DZ","":"DZ","":"E","":"E","":"E","":"E","":"E","":"E","":"E","":"E","":"E","":"E","":"E","":"E","":"E","":"E","":"E","":"E","":"E","":"E","":"E","":"E","":"E","":"E","":"E","":"E","":"E","":"E","":"ET","":"F","":"F","":"G","":"G","":"G","":"G","":"G","":"G","":"G","":"G","":"G","":"H","":"H","":"H","":"H","":"H","":"H","":"H","":"H","":"H","":"I","":"I","":"I","":"I","":"I","":"I","":"I","":"I","":"I","":"I","":"I","":"I","":"I","":"I","":"I","":"I","":"I","":"D","":"F","":"G","":"R","":"S","":"T","":"IS","":"J","":"J","":"K","":"K","":"K","":"K","":"K","":"K","":"K","":"K","":"K","":"K","":"L","":"L","":"L","":"L","":"L","":"L","":"L","":"L","":"L","":"L","":"L","":"L","":"L","":"L","":"LJ","":"M","":"M","":"M","":"M","":"N","":"N","":"N","":"N","":"N","":"N","":"N","":"N","":"N","":"N","":"N","":"N","":"NJ","":"O","":"O","":"O","":"O","":"O","":"O","":"O","":"O","":"O","":"O","":"O","":"O","":"O","":"O","":"O","":"O","":"O","":"O","":"O","":"O","":"O","":"O","":"O","":"O","":"O","":"O","":"O","":"O","":"O","":"O","":"O","":"O","":"O","":"O","":"O","":"O","":"O","":"O","":"O","":"OI","":"OO","":"E","":"O","":"OU","":"P","":"P","":"P","":"P","":"P","":"P","":"P","":"Q","":"Q","":"R","":"R","":"R","":"R","":"R","":"R","":"R","":"R","":"R","":"R","":"R","":"C","":"E","":"S","":"S","":"S","":"S","":"S","":"S","":"S","":"S","":"S","":"S","":"T","":"T","":"T","":"T","":"T","":"T","":"T","":"T","":"T","":"T","":"T","":"A","":"L","":"M","":"V","":"TZ","":"U","